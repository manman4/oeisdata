%o A057752 from sympy import N, li, primepi, floor
%o A057752 def round(n):
%o A057752     return int(floor(n+0.5))
%o A057752 def A057752(n):
%o A057752     return round(N(li(10**n),10*n)) - primepi(10**n) # _Chai Wah Wu_, Apr 30 2018
