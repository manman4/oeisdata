%F A176244 T(n,k) = T(n-1, k-1) + q^k*T(n-1, k), with q=4.
%e A176244 Triangle starts as:
%e A176244   1;
%e A176244   1,        1;
%e A176244   1,       17,          1;
%e A176244   1,      273,         81,          1;
