%e A176244   1, 17895697, 1454007633, 6124779089, 1544878673, 23837265, 21841, 1;
%p A176244 T:= proc(n, k) option remember;
%p A176244     q:=4;
%p A176244       if k=1 or k=n then 1
%p A176244     else T(n-1, k-1) + q^k*T(n-1, k)
%p A176244       fi; end:
%p A176244 seq(seq(T(n, k), k=1..n), n=1..12); # _G. C. Greubel_, Nov 22 2019
%t A176244 q:=4; T[n_, k_]:= T[n, k]= If[k==1 || k==n, 1, q^k*T[n-1, k] + T[n-1, k-1]];
%t A176244 Table[T[n, k], {n,12}, {k,n}]//Flatten (* _G. C. Greubel_, Nov 22 2019 *)
%o A176244 (PARI) T(n,k) = my(q=4); if(k==1 || k==n, 1, q^k*T(n-1,k) + T(n-1,k-1)); \\ _G. C. Greubel_, Nov 22 2019
%o A176244 (Magma)
%o A176244 function T(n,k)
%o A176244   q:=4;
%o A176244   if k eq 1 or k eq n then return 1;
%o A176244   else return T(n-1,k-1) + q^k*T(n-1,k);
%o A176244   end if; return T; end function;
%o A176244 [T(n,k): k in [1..n], n in [1..12]]; // _G. C. Greubel_, Nov 22 2019
%o A176244 (SageMath)
%o A176244 @CachedFunction
%o A176244 def T(n, k):
%o A176244     q=4;
%o A176244     if (k==1 or k==n): return 1
%o A176244     else: return q^k*T(n-1, k) + T(n-1, k-1)
