%p A176283 seq(seq(, k=0..n), n=0..12); # _G. C. Greubel_, Nov 25 2019
%t A176283 (* Set of sequences q=1..10. This sequence is q=3. *)
%t A176283 f[n_, k_, q_]:= f[n, k, q] = 1 + Sum[i^q, {i,0,n}] - Sum[i^q, {i,0,k}] - Sum[i^q, {i,0,n-k}]; Table[Flatten[Table[f[n, k, q], {n,0,10}, {k,0,n}]], {q,1,10}]
%t A176283 (* Second program *)
%t A176283 Table[(4 +n^2*(n+1)^2 -k^2*(k+1)^2 -(n-k)^2*(n-k+1)^2)/4, {n,0,12}, {k,0, n} ]//Flatten (* _G. C. Greubel_, Nov 25 2019 *)
%o A176283 (PARI) T(n,k) = 1 + (n^2*(n+1)^2 - k^2*(k+1)^2 - (n-k)^2*(n-k+1)^2)/4; \\ _G. C. Greubel_, Nov 25 2019
%o A176283 (Magma) [(4 +n^2*(n+1)^2 -k^2*(k+1)^2 -(n-k)^2*(n-k+1)^2)/4: k in [0..n], n in [0..12]]; // _G. C. Greubel_, Nov 25 2019
%o A176283 (SageMath) [[(4 +n^2*(n+1)^2 -k^2*(k+1)^2 -(n-k)^2*(n-k+1)^2)/4 for k in (0..n)] for n in (0..12)] # _G. C. Greubel_, Nov 25 2019
