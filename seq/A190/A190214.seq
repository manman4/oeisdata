%o A190214 (Maxima)
%o A190214 a(n):=sum(sum((sum(binomial(k,r-k)*sum(binomial(j,-3*m+k+2*j)*binomial(m,j),j,0,m),k,m,r))*binomial(-r+n+m-1,m-1),r,m,n),m,1,n);
