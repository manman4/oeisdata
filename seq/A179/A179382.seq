%o A179382 (SageMath)
%o A179382 def A179382(n):
%o A179382     N, o, s = 2*n-1, 1, 0
%o A179382     while True:
%o A179382         o = (N + o) >> valuation(N + o, 2)
%o A179382         s = s + 1
%o A179382         if o == 1: break
%o A179382     return s
