%t A327875 f[p_, e_] := If[e==1, p+1, 1]; s[n_] := Times @@ f @@@ FactorInteger[n]; s1=0; seq = {}; Do[s2 = s[n]; If[s1 == s2, AppendTo[seq, n-1]]; s1 = s2, {n,1,10000}]; seq
%o A327875 (PARI) s(n)={sumdiv(n, d, d*issquarefree(d)*(gcd(d, n/d) == 1))}
