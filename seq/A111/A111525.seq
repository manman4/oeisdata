%t A111525 a[1] = 10; a[n_] := a[n] = Block[{k = 1, c = IntegerDigits @ Table[ a[i], {i, n - 1}]}, While[ !PrimeQ[ FromDigits @ Flatten @ Append[c, IntegerDigits[k]]], k += 2]; k]; Table[ a[n], {n, 63}]
%t A111525 nxt[{j_,a_}]:=Module[{k=1},While[CompositeQ[j*10^IntegerLength[k]+k],k++];{j*10^IntegerLength[k]+k,k}]; NestList[nxt,{10,10},70][[;;,2]] (* _Harvey P. Dale_, Sep 27 2025 *)
