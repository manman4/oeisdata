%p A111594 # The function BellMatrix is defined in A264428.
%p A111594 BellMatrix(n -> `if`(n::even, n!, 0), 10); # _Peter Luschny_, Jan 27 2016
%t A111594 rows = 10;
%t A111594 t = Table[If[EvenQ[n], n!, 0], {n, 0, rows}];
%t A111594 T[n_, k_] := BellY[n, k, t];
