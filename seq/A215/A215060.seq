%e A215060 [9] [-1513, 0, 0, 1596, 0, 0, -84, 0, 0, 1]
%o A215060 (SageMath)
%o A215060 def triangle(f, dim):
%o A215060     var('x,z')
%o A215060     s = f.series(x, dim+2)
%o A215060     P = [factorial(i)*s.coefficient(x,i) for i in range(dim)]
%o A215060     for k in range(dim): print([k], [P[k].coefficient(z,i) for i in (0..k)])
%o A215060 def A215060_triangle(dim) :
%o A215060     var('x, z')
%o A215060     f = exp(x*(z+1/2))/((exp(3*x/2)+2*cos(sqrt(3)*x/2))/3)
%o A215060     return triangle(f, dim)
%o A215060 A215060_triangle(12)
