%p A002385 ff := proc(n) local i,j,k,s,aa,nn,bb,flag; s := n; aa := convert(s,string); nn := length(aa); bb := ``; for i from nn by -1 to 1 do bb := cat(bb,substring(aa,i..i)); od; flag := 0; for j from 1 to nn do if substring(aa,j..j)<>substring(bb,j..j) then flag := 1 fi; od; RETURN(flag); end; gg := proc(i) if ff(ithprime(i)) = 0 then RETURN(ithprime(i)) fi end;
%p A002385 rev:=proc(n) local nn, nnn: nn:=convert(n,base,10): add(nn[nops(nn)+1-j]*10^(j-1),j=1..nops(nn)) end: a:=proc(n) if n=rev(n) and isprime(n)=true then n else fi end: seq(a(n),n=1..20000); # rev is a Maple program to revert a number - _Emeric Deutsch_, Mar 25 2007
%p A002385 # A002385 Gets all base-10 palindromic primes with exactly d digits, in the list "Res"
%p A002385 d:=7; # (say)
%p A002385 if d=1 then Res:= [2,3,5,7]:
%p A002385 elif d=2 then Res:= [11]:
%p A002385 elif d::even then
%p A002385     Res:=[]:
%p A002385 else
%p A002385     m:= (d-1)/2:
%p A002385     Res2 := [seq(seq(n*10^(m+1)+y*10^m+digrev(n), y=0..9), n=10^(m-1)..10^m-1)]:
%p A002385     Res:=[]: for x in Res2 do if isprime(x) then Res:=[op(Res),x]; fi: od:
%p A002385 fi:
%p A002385 Res; # _N. J. A. Sloane_, Oct 18 2015
%t A002385 Select[ Prime[ Range[2100] ], IntegerDigits[#] == Reverse[ IntegerDigits[#] ] & ]
%t A002385 lst = {}; e = 3; Do[p = n*10^(IntegerLength[n] - 1) + FromDigits@Rest@Reverse@IntegerDigits[n]; If[PrimeQ[p], AppendTo[lst, p]], {n, 10^e - 1}]; Insert[lst, 11, 5] (* _Arkadiusz Wesolowski_, May 04 2012 *)
%t A002385 Join[{2,3,5,7,11},Flatten[Table[Select[Prime[Range[PrimePi[ 10^(2n)]+1, PrimePi[ 10^(2n+1)]]],# == IntegerReverse[#]&],{n,3}]]] (* The program uses the IntegerReverse function from Mathematica version 10 *) (* _Harvey P. Dale_, Apr 22 2016 *)
%t A002385 genPal[n_Integer, base_Integer: 10] := Block[{id = IntegerDigits[n, base], insert = Join[{{}}, {# - 1} & /@ Range[base]]}, FromDigits[#, base] & /@ (Join[id, #, Reverse@id] & /@ insert)]; k = 1; lst = {2, 3, 5, 7}; While[k < 19, p = Select[genPal[k], PrimeQ];
%t A002385 If[p != {}, AppendTo[lst, p]]; k++]; Flatten@ lst (* RGWv *)
%t A002385 Select[ Prime[ Range[2100]], PalindromeQ] (* _Jean-François Alcover_, Feb 17 2018 *)
%t A002385 NestList[NestWhile[NextPrime, #, ! PalindromeQ[#2] &, 2] &, 2, 41] (* _Jan Mangaldan_, Jul 01 2020 *)
