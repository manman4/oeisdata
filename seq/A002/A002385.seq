%o A002385 (Python)
%o A002385 from sympy import isprime
%o A002385 from itertools import count, islice, product
%o A002385 def A002385gen(): # generator of palprimes
%o A002385     yield from [2, 3, 5, 7, 11]
%o A002385     for d in count(3, 2):
%o A002385         for last in "1379":
%o A002385             for p in product("0123456789", repeat=d//2-1):
%o A002385                 left = "".join(p)
%o A002385                 for mid in [[""], "0123456789"][d&1]:
%o A002385                     t = int(last + left + mid + left[::-1] + last)
%o A002385                     if isprime(t):
%o A002385                         yield t
%o A002385 print(list(islice(A002385gen(), 46))) # _Michael S. Branicky_, Apr 13 2025
