%p A002145 A002145 := proc(n)
%p A002145     option remember;
%p A002145     if n = 1 then
%p A002145         3;
%p A002145     else
%p A002145         a := nextprime(procname(n-1)) ;
%p A002145         while a mod 4 <>  3 do
%p A002145             a := nextprime(a) ;
%p A002145         end do;
%p A002145         return a;
%p A002145     end if;
%p A002145 end proc:
