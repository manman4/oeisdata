%t A123958 M = {{0, 1, 0}, {0, 0, 1}, {-2, -2, 0}}; v[1] = {0, 0, 1}; v[n_] := v[n] = M.v[n - 1]; a1 = Table[v[n][[1]], {n, 1, 50}]
