%F A123879 a(n) = Sum_{k=0..n} Sum_{j=0..n} (-1)^(j-k)*C(n+j,2*j)*C(j+k,2*k).
%p A123879 seq(coeff(series((1-2*x+2*x^2-x^3)/(1-3*x+5*x^2-3*x^3+x^4), x, n+1), x, n), n = 0 .. 40); # _G. C. Greubel_, Aug 08 2019
