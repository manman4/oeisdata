%o A385991 def A385991(n):
%o A385991     if n==0: return 1
%o A385991     a, b, s, c = 0, 1, {0,1}, 2
%o A385991     for i in range(n-1):
%o A385991         a, b = b, ((a//b<<1)+1)*b-a
%o A385991         if b not in s:
%o A385991             c += 1
%o A385991             s.add(b)
%o A385991     return c # _Chai Wah Wu_, Jul 17 2025
