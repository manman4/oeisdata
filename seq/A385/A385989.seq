%t A385989 a[n_]:=Module[{m=n+1},While[PowerMod[2,n,n]!=PowerMod[2,m,n], m++]; m]; Array[a,66] (* _Stefano Spezia_, Jul 16 2025 *)
%o A385989 (PARI) a(n) = { my (u = Mod(2, n)^n, v = u); for (m = n+1, oo, if (u==v*=2, return (m));); }
