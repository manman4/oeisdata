%I A385964 #33 Sep 15 2025 00:58:46
%S A385964 4,2,1,11,35,71,791,2015,1259,9359,7559,15590,42839,75599,110159,
%T A385964 251999,262079,576575,393119,453599,665279,1713599
%N A385964 a(n) is the least number that occurs exactly n times in A075255.
%e A385964 a(3) = 11 because A075255(20) = A075255(21) = A075255(26) = 11, i.e. 11 = 20 - 2*2 - 5 = 21 - 3 - 7 = 26 - 2 - 13, and no number smaller than 11 works.
%p A385964 sopfr:= proc(n) local F, t;
%p A385964   F:= ifactors(n)[2];
%p A385964   add(t[1]*t[2], t=F)
%p A385964 end proc:
%p A385964 M:= 2*10^6: Mh:=M/2-2:
%p A385964 g:= proc(t) local v; v:= t - sopfr(t); if v > 0 and v < Mh then v fi end proc:
%p A385964 R:= sort(map(g, [$1..M])):
%p A385964 V:= Array(0..20):
%p A385964 V[0]:= 4:
%p A385964 state:= 1: r:= R[1]:
%p A385964 for i from 2 to nops(R) do
%p A385964   if R[i] = r then
%p A385964     state:= state+1
%p A385964   else
%p A385964     if state <= 20 and V[state] = 0 then V[state]:= r fi;
%p A385964     state:= 1: r:= R[i]
%p A385964   fi
%p A385964 od:
%p A385964 convert(V,list);
%Y A385964 Cf. A075255, A388132.
%K A385964 nonn,more
%O A385964 0,1
