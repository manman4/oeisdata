%o A157151 def T(n,k,m): # A157147
%o A157151     if (k==0 or k==n): return 1
%o A157151     else: return (m*(n-k) +1)*T(n-1,k-1,m) + (m*k+1)*T(n-1,k,m) + m*k*(n-k)*T(n-2,k-1,m)
