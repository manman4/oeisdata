%o A157148 def T(n,k,m):  # A157148
%o A157148     if (k==0 or k==n): return 1
%o A157148     else: return (m*(n-k) +1)*T(n-1,k-1,m) + (m*k+1)*T(n-1,k,m) + m*k*(n-k)*T(n-2,k-1,m)
