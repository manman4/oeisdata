%p A157149 seq(seq(A157149(n,k),k=0..n),n=0..10) ; # _R. J. Mathar_, Feb 06 2015
%t A157149 T[n_, k_, m_]:= T[n, k, m]= If[k==0 || k==n, 1, (m*(n-k)+1)*T[n-1,k-1,m] + (m*k+1)*T[n-1,k,m] + m*k*(n-k)*T[n-2,k-1,m]];
