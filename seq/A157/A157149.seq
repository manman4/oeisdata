%p A157149 A157149 := proc(n,k)
%p A157149     option remember;
%p A157149     if k < 0 or k> n then 0;
%p A157149     elif k = 0 or k = n then 1;
%p A157149     else (3*(n-k)+1)*procname(n-1,k-1) + (3*k+1)*procname(n-1,k) + 3*k*(n-k)*procname(n-2,k-1);
%p A157149     end if;
