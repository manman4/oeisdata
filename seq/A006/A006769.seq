%t A006769 a[ n_] := Which[n<0, -a[-n], n<5, {0, 1, 1, -1, 1}[[1+n]], True, (a[n-1]*a[n-3] + a[n-2]^2)/a[n-4]]; (* _Michael Somos_, Aug 20 2024 *)
