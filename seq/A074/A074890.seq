%F A074890 a(n+1) = A387312(a(n)). - _Mia Boudreau_, Sep 10 2025
%e A074890 Initial terms:
%e A074890   1 (1)
%e A074890   11 (3)
%e A074890   110 (6)
%e A074890   1101 (13)
%e A074890   11001 (25)
%e A074890   110111 (55)
%o A074890 (Java)
%o A074890 BigInteger a(int n){
%o A074890  BigInteger k = BigInteger.ONE;
%o A074890  for(int t = 0; t < n; t++){
%o A074890   int RULE = 30;
%o A074890   BigInteger x = BigInteger.ZERO;
%o A074890   for(int i = 0; i < n+1; i++)
%o A074890    if((RULE>>((gb(k,i+1)<<2)|(gb(k,i)<<1)|gb(k,i-1))&1)>0) x=x.setBit(i);
%o A074890   k = x;}
%o A074890  return k;}
%o A074890 int gb(BigInteger b, int i){return b.shiftRight(i).testBit(0)?1:0;}} // _Mia Boudreau_, Sep 10 2025
