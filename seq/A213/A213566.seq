%o A213566 for(n=1,12, for(k=1,n, print1(k*(k*f(n-k+3) +2*f(n-k+4)) + f(n-k+7) -(k+2)*(2*n-k+4) -(n-k+1)^2 -4, ", "))) \\ _G. C. Greubel_, Jul 26 2019
