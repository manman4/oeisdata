%t A096491 u=1;Do[s=Max[Last[ContinuedFraction[n^(1/2)]]];tc[[u]]=s;u=u+1, {n, 1, m}]
%t A096491 psq[n_]:=With[{s=Sqrt[n]},If[IntegerQ[s],s,Max[ContinuedFraction[s][[2]]]]]; Array[psq,80] (* _Harvey P. Dale_, Oct 26 2025 *)
