%t A173522 f[n_] := f[n] = Sum[ Binomial[1, n - k]Mod[ Binomial[k, j], 2], {k, 0, n}, {j, 0, k}]; g[n_] := Sum[ f@k, {k, 0, n}]; Array[g, 55, 0] (* _Robert G. Wilson v_, Jun 28 2010 *)
