%p A001790 swing := proc(n) option remember; if n = 0 then 1 elif irem(n, 2) = 1 then swing(n-1)*n else 4*swing(n-1)/n fi end:
%p A001790 sigma := n -> 2^(add(i,i=convert(iquo(n,2),base,2))):
%p A001790 a := n -> swing(2*n)/sigma(2*n); # (End)
