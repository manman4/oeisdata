%o A001850 (Python) # from Nick Hobson.
%o A001850 def f(a, b):
%o A001850     if a == 0 or b == 0:
%o A001850         return 1
%o A001850     return f(a, b - 1) + f(a - 1, b) + f(a - 1, b - 1)
%o A001850 [f(n, n) for n in range(7)]
