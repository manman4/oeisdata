%o A001694 (Python)
%o A001694 from math import isqrt
%o A001694 from sympy import mobius, integer_nthroot
%o A001694 def A001694(n):
%o A001694     def squarefreepi(n):
%o A001694         return int(sum(mobius(k)*(n//k**2) for k in range(1, isqrt(n)+1)))
%o A001694     def bisection(f,kmin=0,kmax=1):
%o A001694         while f(kmax) > kmax: kmax <<= 1
%o A001694         while kmax-kmin > 1:
%o A001694             kmid = kmax+kmin>>1
%o A001694             if f(kmid) <= kmid:
%o A001694                 kmax = kmid
%o A001694             else:
%o A001694                 kmin = kmid
%o A001694         return kmax
%o A001694     def f(x):
%o A001694         c, l = n+x, 0
%o A001694         j = isqrt(x)
%o A001694         while j>1:
%o A001694             k2 = integer_nthroot(x//j**2,3)[0]+1
%o A001694             w = squarefreepi(k2-1)
%o A001694             c -= j*(w-l)
%o A001694             l, j = w, isqrt(x//k2**3)
%o A001694         c -= squarefreepi(integer_nthroot(x,3)[0])-l
%o A001694         return c
%o A001694     return bisection(f,n,n) # _Chai Wah Wu_, Sep 09 2024
