%C A001181      Starting meander:  /\  /\  / /\ \   Split and    /\/\//\\ ///\\/\\
%C A001181                         \ \/  \ \/ / /   rotate
%C A001181                          \     \  / /    bottom arches    /\
%C A001181                           \     \/ /                     /  \           arches= 7
%C A001181                            \      /                     /  /\\   /\
%C A001181                             \    /    Combine start    //\//\\\ //\\/\
%C A001181                              \  /     first arch with
%C A001181       meander:                \/      end of last arch
%C A001181           /\                                                       /\
%C A001181    /\    /  \                         Combine arches        /\    //\\  arches= 6
%C A001181    \ \  / /\ \                        again then        /\ //\\  ///\\\
%C A001181     \ \ \/ / /                        rotate and join
%C A001181      \ \  / /                         <--                  /\
%C A001181       \ \/ /                                              //\\   /\     arches= 5
%C A001181        \  /                           Combine            ///\\\ //\\
%C A001181         \/         /\
%C A001181        meander:   /  \
%C A001181                  / /\ \               Combine              /\
%C A001181                  \/  \/               <--                 //\\ /\/\     arches= 4
%C A001181                                                                /\
%C A001181                                       Combine              /\ //\\      arches= 3
%C A001181       meander:  /\                    Combine
%C A001181                 \/                    <--                   /\ /\ arches= 2
