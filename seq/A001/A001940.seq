%t A001940 nn = 4*10; b = Flatten[Table[{6, 6, 6, 0}, {nn/4}]]; CoefficientList[x*Series[Product[1/(1 - x^m)^b[[m]], {m, nn}], {x, 0, nn}], x] (* _T. D. Noe_, Aug 17 2012 *)
