%N A026593 a(n) = T(2*n-1, n-1), where T is given by A026584.
%H A026593 G. C. Greubel, <a href="/A026593/b026593.txt">Table of n, a(n) for n = 1..1000</a>
%F A026593 a(n) = A026584(2*n-1, n-1).
%t A026593 T[n_, k_]:= T[n, k]= If[k==0 || k==2*n, 1, If[k==1 || k==2*n-1, Floor[n/2], If[EvenQ[n+k], T[n-1, k-2] + T[n-1, k], T[n-1, k-2] + T[n-1, k-1] + T[n-1, k] ]]]; (* T = A026584 *)
%t A026593 a[n_]:= a[n]= Block[{$RecursionLimit= Infinity}, T[2*n-1,n-1]];
%t A026593 Table[a[n], {n, 1, 40}] (* _G. C. Greubel_, Dec 13 2021 *)
%o A026593 (SageMath)
%o A026593 @CachedFunction
%o A026593 def T(n, k):  # T = A026584
%o A026593     if (k==0 or k==2*n): return 1
%o A026593     elif (k==1 or k==2*n-1): return (n//2)
%o A026593     else: return T(n-1, k-2) + T(n-1, k) if ((n+k)%2==0) else T(n-1, k-2) + T(n-1, k-1) + T(n-1, k)
%o A026593 [T(2*n-1, n-1) for n in (1..40)] # _G. C. Greubel_, Dec 13 2021
