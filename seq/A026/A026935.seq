%t A026935 A008288[n_, k_]:= Binomial[n, k]*Hypergeometric2F1[-k, k-n, -n, -1];
%t A026935 A026935[n_]:= Sum[A008288[n, k]*A008288[n, k+2], {k, 0, n-2}];
