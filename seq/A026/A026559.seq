%N A026559 a(n) = T(2*n, n-1), where T is given by A026552.
%H A026559 G. C. Greubel, <a href="/A026559/b026559.txt">Table of n, a(n) for n = 1..1000</a>
%F A026559 a(n) = A026552(2*n, n-1)
%t A026559 T[n_, k_]:= T[n, k]= If[k==0 || k==2*n, 1, If[k==1 || k==2*n-1, Floor[(n+2)/2], If[EvenQ[n], T[n-1, k-2] + T[n-1, k] + T[n-1, k-1], T[n-1, k-2] + T[n-1, k]]]]; (* T=A026552 *)
%t A026559 a[n_]:= a[n]= Block[{$RecursionLimit = Infinity}, T[2*n, n-1]];
%t A026559 Table[a[n], {n,40}] (* _G. C. Greubel_, Dec 17 2021 *)
%o A026559 (SageMath)
%o A026559 @CachedFunction
%o A026559 def T(n,k): # T = A026552
%o A026559     if (k==0 or k==2*n): return 1
%o A026559     elif (k==1 or k==2*n-1): return (n+2)//2
%o A026559     elif (n%2==0): return T(n-1, k) + T(n-1, k-1) + T(n-1, k-2)
%o A026559     else: return T(n-1, k) + T(n-1, k-2)
%o A026559 [T(2*n,n-1) for n in (1..40)] # _G. C. Greubel_, Dec 17 2021
