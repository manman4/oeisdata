%t A026737 T[_, 0]=T[n_, n_]=1; T[n_, k_]:= T[n, k]= If[EvenQ[n] && k==(n-2)/2, T[n-1, k-1] + T[n-2, k-1] + T[n-1, k], T[n-1, k-1] + T[n-1, k]];
%t A026737 a[n_] := T[2n, n];
