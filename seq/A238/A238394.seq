%t A238394 a[n_] := Length@ Select[ IntegerPartitions@n, 0 < Min@ Abs[ Reverse@# - Range@ Length@#] &]; Array[a, 30]
%t A238394 b[n_, i_] := b[n, i] = If[n==0, 1, If[i<1, 0, b[n, i-1] + If[i>n, 0, Function[p, Expand[x*(p-Coefficient[p, x, i-1]*x^(i-1))]][b[n-i, i]]]]]; a[n_] := Function[p, Sum[Coefficient[p, x, i], {i, 0, Exponent[p, x]} ] ][b[n, n]]; Table[a[n], {n, 0, 70}] (* _Jean-Fran√ßois Alcover_, Nov 02 2015, after _Alois P. Heinz_ *)
