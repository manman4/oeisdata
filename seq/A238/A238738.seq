%o A238738 (Magma) m:=60; R<x>:=PowerSeriesRing(Integers(), m); Coefficients(R!((1+2*x+2*x^2)/(1-x-2*x^3+2*x^4+x^6-x^7)));
%o A238738 (Maxima) makelist(coeff(taylor((1+2*x+2*x^2)/(1-x-2*x^3+2*x^4+x^6-x^7), x, 0, n), x, n), n, 0, 60);
