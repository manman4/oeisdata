%o A261588         f = ((1-x^k)/(1-x))^n # ((x+1)^2-x^2*(x/(x+1))^(k-2))^n
%o A261588         f = f.simplify_full()
%o A261588         C = 0
%o A261588         for i in range(n):
%o A261588             C = C + (n-i)*f.coefficient(x,i)/n
%o A261588         print(C)
