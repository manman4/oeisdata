%e A056472 11;
%e A056472 12,2,6,2,2,3,3,4;
%o A056472 (SageMath)
%o A056472 def factorizations(n) : return fact_helper(n, 2)
%o A056472 def fact_helper(n, min) : return sum(([[d] + F for F in fact_helper(n//d, d)] for d in divisors(n) if d >= min and d <= isqrt(n)), [[n]])
