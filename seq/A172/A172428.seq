%t A172428 f[n_, q_]:= ((1-q^n)*(1+(-1)^n) + n!*(1-(-1)^n))/2;
%t A172428 c[n_, q_]:= Product[f[j, q], {j, n}];
%t A172428 T[n_, k_, q_]:= c[n, q]/(c[k, q]*c[n-k, q]);
%t A172428 Table[T[n, k, 3], {n,0,12}, {k,0,n}]//Flatten (* modified by _G. C. Greubel_, May 07 2021 *)
%o A172428 (SageMath)
%o A172428 @CachedFunction
%o A172428 def f(n,q): return ((1-q^n)*(1+(-1)^n) + factorial(n)*(1-(-1)^n))/2
%o A172428 def c(n,q): return product( f(j,q) for j in (1..n) )
%o A172428 def T(n,k,q): return c(n,q)/(c(k,q)*c(n-k,q))
%o A172428 flatten([[T(n,k,3) for k in (0..n)] for n in (0..12)]) # _G. C. Greubel_, May 07 2021
