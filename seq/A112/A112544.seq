%o A112544 (PARI)
%o A112544 t1(n) = binomial(floor(3/2+sqrt(2*n)),2) -n+1;
%o A112544 t2(n) = n-binomial(floor(1/2+sqrt(2*n)),2);
%o A112544 vector(100,n,t2(n)/gcd(t1(n),t2(n)))
