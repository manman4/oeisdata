%t A182758 nxt[{a_,b_}]:=Module[{h=b+1},While[!IntegerQ[((a+b)(a+h)(b+h))/ (a+b+h)], h++];{b,h}]; Transpose[NestList[nxt,{1,2},40]][[1]] (* _Harvey P. Dale_, Dec 13 2012 *)
%o A182758 (SageMath)
%o A182758 works = lambda a, h: ((a[-2]+a[-1])*(a[-2]+h)*(a[-1]+h)/(a[-2]+a[-1]+h)).is_integral()
