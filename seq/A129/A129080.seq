%t A129080 (* b = A095263 *)
%t A129080 b[n_]:= b[n]= If[n<4, 2^n -1, 3*b[n-1] -2*b[n-2] +b[n-3]];
%t A129080 a[n_]:= a[n]= If[n==1, 4, a[n-1] +b[n] -b[n-1] +2];
