%N A156883 Square array T(n, k) = Product_{j=1..n} ( Sum_{i=0..j-1} ((k+1)^4 - (k+1)^3)^i ) with T(n, 0) = n!, read by antidiagonals.
%H A156883 G. C. Greubel, <a href="/A156883/b156883.txt">Antidiagonal rows n = 0..25, flattened</a>
%F A156883 T(n, k) = Product_{j=1..n} ( Sum_{i=0..j-1} ((k+1)^4 - (k+1)^3)^i ) with T(n, 0) = n! (square array).
%F A156883 T(n, k) = ( Product_{j=1..n} (k^j*(k+1)^(3*j) -1) )/(k*(k+1)^3 -1)^n with T(n, 0) = n! (square array).
%e A156883 Square array begins as:
%e A156883    1,      1,           1,              1, ...;
%e A156883    1,      1,           1,              1, ...;
%e A156883    2,      9,          55,            193, ...;
%e A156883    6,    657,      163405,        7152001, ...;
%e A156883   24, 384345, 26215881175, 50886093754945, ...;
%e A156883 Triangle begins as:
%e A156883   1;
%e A156883   1, 1;
%e A156883   1, 1,    2;
%e A156883   1, 1,    9,         6;
%e A156883   1, 1,   55,       657,             24;
%e A156883   1, 1,  193,    163405,         384345,                120;
%e A156883   1, 1,  501,   7152001,    26215881175,         1799118945,            720;
%e A156883   1, 1, 1081, 125501001, 50886093754945, 227121050616681925, 67375205371305, 5040;
%t A156883 (* First program *)
%t A156883 T[n_, m_] = If[m==0, n!, Product[Sum[(-(m+1)^3 + (m+1)^4)^i, {i,0,k-1}], {k,n}]];
%t A156883 Table[T[k,n-k], {n,0,12}, {k,0,n}]//Flatten (* modified by _G. C. Greubel_, Jun 12 2021 *)
%t A156883 (* Second program *)
%t A156883 T[n_, k_]= If[k==0, n!, Product[(k^j*(k+1)^(3*j) -1), {j,n}]/(k*(k+1)^3 -1)^n];
%t A156883 Table[T[k, n-k], {n,0,12}, {k,0,n}]//Flatten (* _G. C. Greubel_, Jun 14 2021 *)
%o A156883 (SageMath)
%o A156883 def A156883(n, k): return factorial(n) if (k==0) else product((k^j*(k+1)^(3*j) -1) for j in (1..n))/(k*(k+1)^3 -1)^n
%o A156883 flatten([[A156883(k,n-k) for k in (0..n)] for n in (0..12)]) # _G. C. Greubel_, Jun 14 2021
