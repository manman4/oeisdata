%t A156691 (* First program *)
%t A156691 t[n_, k_]:= If[k==0, n!, Product[1 -(i+1)*(k+1), {j,n}, {i,0,j-1}] ];
%t A156691 T[n_, k_, m_]:= If[n==0, 1, t[n, m]/(t[k, m]*t[n-k, m])];
