%t A156578 T[n_]:= If[n==0, 0, CoefficientList[1 -(n+1)*x^n +n*x^(n+1), x]];
%t A156578 Table[T[n], {n,0,15}]//Flatten (* modified by _G. C. Greubel_, Jan 03 2022 *)
