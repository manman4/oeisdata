%F A156577 a(n) = Sum_{k=0..n} A120730(n,k) * 9^k.
%t A156577 a[n_]:= a[n]= If[n==0, 1, If[OddQ[n], 10*a[n-1] -9^((n-1)/2)*CatalanNumber[(n-1)/2], 10*a[n-1] ]];
%t A156577 Table[a[n], {n, 0, 30}] (* _G. C. Greubel_, Jan 04 2022 *)
%o A156577 (SageMath)
%o A156577 def a(n): # a = A156577
%o A156577     if (n==0): return 1
%o A156577     elif (n%2==1): return 10*a(n-1) - 9^((n-1)/2)*catalan_number((n-1)/2)
%o A156577     else: return 10*a(n-1)
%o A156577 [a(n) for n in (0..30)] # _G. C. Greubel_, Jan 04 2022
