%o A288485 def A288485(n)
%o A288485   a = [0] + (1..n).map{|i| A001158(i)}
%o A288485   ary = a.clone
%o A288485   (1..n).each{|i|
%o A288485     ary[i] -= 28 * a[i / 2] if i % 2 == 0
%o A288485     ary[i] += 63 * a[i / 3] if i % 3 == 0
%o A288485     ary[i] -= 36 * a[i / 6] if i % 6 == 0
%o A288485   }
%o A288485   ary[1..-1]
%o A288485 end
