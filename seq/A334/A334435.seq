%t A334435 revlensort[f_,c_]:=If[Length[f]!=Length[c],Length[f]<Length[c],OrderedQ[{c,f}]];
%t A334435 Table[Times@@Prime/@#&/@Sort[Sort/@IntegerPartitions[n],revlensort],{n,0,8}]
%o A334435 (PARI)
%o A334435 cmpLenLexDesc(x,y)=my(d=cmp(#x,#y)); if(d, d, -lex(x,y))
%o A334435 C(sig)=prod(k=1,#sig,prime(sig[k]))
%o A334435 Row(n)={apply(C, vecsort([p | p<-partitions(n)], cmpLenLexDesc))}
%o A334435 { for(n=0, 9, print(Row(n))) } \\ _Andrew Howroyd_, Oct 07 2025
