%t A334301 Join@@Table[Sort[IntegerPartitions[n]],{n,0,8}]
%o A334301 (PARI)
%o A334301 cmpLenLexAsc(x,y)=my(d=cmp(#x,#y)); if(d, d, lex(x,y))
%o A334301 Row(n)={concat(vecsort([Vecrev(p) | p<-partitions(n)], cmpLenLexAsc))}
