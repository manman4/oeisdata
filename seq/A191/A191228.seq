%t A191228 nn = 100; R = Table[0, {nn}]; s = 0;
%t A191228 Do[If[PrimeQ[k], s++]; If[PrimeQ[k/2], s--]; If[s < nn, R[[s + 1]] = k], {k, Prime[3 nn]}];
%t A191228 A104272 = R + 1;
