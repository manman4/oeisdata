%p A153641 # Recursion
%p A153641 W := proc(n,z) option remember; local k,p;
%p A153641 if n = 0 then 1 else p := irem(n+1,2);
%p A153641 z^n - p + add(`if`(irem(k,2)=1,0,
%p A153641 W(k,0)*binomial(n,k)*(power(z,n-k)-p)),k=2..n-1) fi end:
