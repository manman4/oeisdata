%t A065619 a[1] = 1; a[n_] := 2^(n-1)*Abs[EulerE[n - 1, 1/2] + EulerE[n - 1, 1]]*n; Array[a, 24] (* _Jean-Fran√ßois Alcover_, Nov 05 2017, after _Peter Luschny_ *)
