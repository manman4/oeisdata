%p A381369      `if`(u+o=0, 1, `if`(t>0, b(u+o, 0$2, k), 0)+add(k^(u+j-1)*
%p A381369         b(o-j, u+j-1, min(2, t+1), k), j=`if`(t=0, 1, 1..o)))
%p A381369     end:
