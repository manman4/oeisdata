%p A178300 gf := y*(1 - (1 - 2*x)/sqrt(1 - 4*x*y) )/(2*(1 - x - y)):
%p A178300 Max := 10: ser := simplify(series(gf, x, Max + 1)):
%p A178300 seq(print(seq(coeff(coeff(ser, x, n), y, k), k = 1..n)), n = 1..Max);  # _Peter Luschny_, Oct 13 2025
%t A178300 m[par_?PartitionQ, v_] := Block[{le = Length[par], it }, If[le > v, Return[0]]; it = Permutations[PadRight[par, v]]; Tr[ Apply[Times, Table[Subscript[x, j], {j, v}]^# & /@ it, {1}]]];
