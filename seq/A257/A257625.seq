%t A257625 t[n_, k_, p_, q_]:= t[n, k, p, q] = If[n<0 || k<0, 0, If[n==0 && k==0, 1, (p*k+q)*t[n-1,k,p,q] + (p*n+q)*t[n,k-1,p,q]]];
%t A257625 T[n_, k_, p_, q_]= t[n-k, k, p, q];
%t A257625 Table[T[n,k,6,3], {n,0,12}, {k,0,n}]//Flatten (* _G. C. Greubel_, Mar 01 2022 *)
%o A257625 (SageMath)
%o A257625 @CachedFunction
%o A257625 def t(n,k,p,q):
%o A257625     if (n<0 or k<0): return 0
%o A257625     elif (n==0 and k==0): return 1
%o A257625     else: return (p*k+q)*t(n-1,k,p,q) + (p*n+q)*t(n,k-1,p,q)
%o A257625 def A257625(n,k): return t(n-k,k,6,3)
