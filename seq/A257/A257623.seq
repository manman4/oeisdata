%t A257623 t[n_, k_, p_, q_]:= t[n, k, p, q]= If[n<0 || k<0, 0, If[n==0 && k==0, 1, (p*k+ q)*t[n-1,k,p,q] + (p*n+q)*t[n,k-1,p,q]]];
%t A257623 T[n_, k_, p_, q_]= t[n-k,k,p,q];
%t A257623 Table[T[n,k,5,3], {n,0,12}, {k,0,n}]//Flatten (* _G. C. Greubel_, Feb 27 2022 *)
%o A257623 (SageMath)
%o A257623 @CachedFunction
%o A257623 def t(n,k,p,q):
%o A257623     if (n<0 or k<0): return 0
%o A257623     elif (n==0 and k==0): return 1
%o A257623     else: return (p*k+q)*t(n-1,k,p,q) + (p*n+q)*t(n,k-1,p,q)
%o A257623 def A257623(n,k): return t(n-k,k,5,3)
