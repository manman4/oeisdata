%t A257610 T[n_, k_, a_, b_]:= T[n, k, a, b]= If[k<0 || k>n, 0, If[n==0, 1, (a*(n-k)+b)*T[n-1, k-1, a, b] + (a*k+b)*T[n-1, k, a, b]]];
%t A257610 Table[T[n,k,3,2], {n,0,12}, {k,0,n}]//Flatten (* _G. C. Greubel_, Mar 20 2022 *)
%o A257610 (SageMath)
%o A257610 def T(n,k,a,b): # A257610
%o A257610     if (k<0 or k>n): return 0
%o A257610     elif (n==0): return 1
%o A257610     else: return  (a*k+b)*T(n-1,k,a,b) + (a*(n-k)+b)*T(n-1,k-1,a,b)
%o A257610 flatten([[T(n,k,3,2) for k in (0..n)] for n in (0..12)]) # _G. C. Greubel_, Mar 20 2022
