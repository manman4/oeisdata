%t A257627 f[n_]:= 7*n+3;
%t A257627 t[n_, k_]:= t[n,k]= If[n<0 || k<0, 0, If[n==0 && k==0, 1, f[k]*t[n-1,k] +f[n]*t[n,k-1]]];
%t A257627 T[n_, k_]= t[n-k, k];
%t A257627 Table[T[n, k], {n,0,12}, {k,0,n}]//Flatten (* _G. C. Greubel_, Feb 22 2022 *)
%o A257627 (SageMath)
%o A257627 def f(n): return 7*n+3
%o A257627 @CachedFunction
%o A257627 def t(n,k):
%o A257627     if (n<0 or k<0): return 0
%o A257627     elif (n==0 and k==0): return 1
%o A257627     else: return f(k)*t(n-1, k) + f(n)*t(n, k-1)
%o A257627 def A257627(n,k): return t(n-k,k)
