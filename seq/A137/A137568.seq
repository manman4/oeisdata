%o A137568 (SageMath)
%o A137568 def A137568(n):
%o A137568     d = {}
%o A137568     for g in graphs(n):
%o A137568         c = g.chromatic_polynomial()
%o A137568         d[c] = 1 if c not in d else d[c]+1
%o A137568     return d.values().count(1)  # _D. S. McNeil_, Jan 02 2011
