%t A137478 f[n_]:= Product[Fibonacci[j], {j, 1, n}]; Table[Binomial[n-1, k-1]* f[n]/f[k], {n, 1, 12}, {k, 1, n}]//Flatten (* _G. C. Greubel_, May 15 2019 *)
%o A137478 (PARI)
%o A137478 {f(n) = prod(j=1,n, fibonacci(j))};
