%o A214551 (Python)
%o A214551 from math import gcd
%o A214551 def aupton(nn):
%o A214551     alst = [1, 1, 1]
%o A214551     for n in range(3, nn+1):
%o A214551         alst.append((alst[n-1] + alst[n-3])//gcd(alst[n-1], alst[n-3]))
%o A214551     return alst
