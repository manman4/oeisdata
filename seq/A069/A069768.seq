%o A069768 (Scheme) ;; Implementations of this automorphism. These act on S-expressions, i.e. list-structures:
%o A069768 ;; Constructive version:
%o A069768 (define (*A069768 s) (cond ((not (pair? s)) s) (else (cons (*A069768 (cdr s)) (car s)))))
%o A069768 ;; Destructive version:
%o A069768 (define (*A069768! s) (cond ((pair? s) (*A069768! (cdr s)) (*A069770! s))) s)
