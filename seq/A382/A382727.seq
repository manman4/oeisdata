%o A382727 from math import prod
%o A382727 from gmpy2 import digits
%o A382727 def A382727(n): return sum(prod(int(d,11)+1 for d in digits(m,11)) for m in range(n+1)) # _Chai Wah Wu_, Aug 10 2025
%o A382727 (Python)
%o A382727 from math import prod
