%t A201638 T[0, 0] = 1; T[n_, n_] = 1; T[n_, k_] /; 0 <= k <= n := T[n, k] = T[n - 1, k - 1] + 3*T[n - 1, k] + 3*T[n - 1, k + 1]; T[_, _] = 0;
%t A201638 Table[T[n, k], {n, 0, 9}, {k, 0, n}] (* _Jean-FranÃ§ois Alcover_, Jun 29 2019 *)
%o A201638 (SageMath)
%o A201638 def A201638_triangle(dim):
%o A201638     T = matrix(ZZ,dim,dim)
%o A201638     for n in range(dim): T[n,n] = 1
%o A201638     for n in (1..dim-1):
%o A201638         for k in (0..n-1):
%o A201638             T[n,k] = T[n-1,k-1]+3*T[n-1,k]+3*T[n-1,k+1]
%o A201638     return T
