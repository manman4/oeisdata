%o A099252 (SageMath)
%o A099252 def A():
%o A099252     a, b, c, d, n = 0, 1, 1, -1, 1
%o A099252     yield 0
%o A099252     while True:
%o A099252         n += 1
%o A099252         a, b = b, (3*(n-1)*n*a+(2*n-1)*n*b)//((n+1)*(n-1))
%o A099252         c, d = d, (3*(n-1)*c-(2*n-1)*d)//n
%o A099252         if n%2: yield -(d + b)*(1-(-1)^n)//2
