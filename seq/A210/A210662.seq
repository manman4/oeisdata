%o A210662 (SageMath)
%o A210662 from sage.combinat.tiling import TilingSolver, Polyomino
%o A210662 def T(n, k):
%o A210662     p = Polyomino([(0, 0)])
%o A210662     q = Polyomino([(0, 0), (0, 1)])
%o A210662     T = TilingSolver([p, q], box=[n, k], reusable=True)
%o A210662     return T.number_of_solutions()
