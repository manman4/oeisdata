%t A127059 A127058[n_, k_]:= A127058[n, k] = If[k==n, n+1, Sum[A127058[j+k, k]* A127058[n-j, k+1], {j,0,n - k - 1}]]; Table[A127058[n+2, 2], {n, 0, 30}] (* _G. C. Greubel_, Jun 09 2019 *)
