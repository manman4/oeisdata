%o A152722 (SageMath)
%o A152722 @CachedFunction
%o A152722 def T(n,k):
%o A152722    if k==n: return -1
%o A152722    elif n==1 and k==0: return 1
%o A152722    elif k==0: return nth_prime(n+2)
%o A152722    elif k==1: return 1 - nth_prime(n+2)
%o A152722    else: return T(n-1,k-1)
