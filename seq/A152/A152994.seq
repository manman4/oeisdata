%o A152994 (Magma) [9*n*(2*n-1): n in [0..40]]; // _G. C. Greubel_, Sep 01 2019
%o A152994 (SageMath) [9*n*(2*n-1) for n in (0..40)] # _G. C. Greubel_, Sep 01 2019
%o A152994 (GAP) List([0..40], n-> 9*n*(2*n-1)); # _G. C. Greubel_, Sep 01 2019
