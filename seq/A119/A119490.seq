%t A119490 A049614[n_]:= n!/Product[Prime[i], {i, 1, PrimePi[n]}];
%t A119490 b:= Join[{{1}}, Table[CoefficientList[Product[1 -A049614[k]*x, {k,0,n}], x], {n, 0, 21}]];
