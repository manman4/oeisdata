%t A154855 T[n_,k_,p_,q_,r_,t_]:= SeriesCoefficient[(1/p)*(1-x)^(n+1)*Sum[((q*j+r)^n - (q*j+t)^n)*x^j, {j, 0, n}], {x,0,k}];
%t A154855 Table[T[n,k,3,5,4,1,3], {n,0,12},{k,0,n}]//Flatten (* modified by _G. C. Greubel_, Mar 11 2021 *)
%o A154855 (SageMath)
%o A154855 def f(n,p,q,r,t,x) : return (1/p)*(1-x)^(n+1)*sum( ((q*j+r)^n - (q*j+t)^n )*x^j for j in (0..n))
%o A154855 [[( f(n,3,5,4,1,x) ).series(x,n+1).list()[k] for k in (0..n)] for n in (0..12)] # _G. C. Greubel_, Mar 11 2021
