%o A384597     return any((k+1)%d==0 and sorted(str(d))==sorted(str(k)) and len(str(d))==len(str(k)) for d in range(1,k+2))
%o A384597 print(", ".join(map(str, [k for k in range(1, 100000) if ok(k)])))
