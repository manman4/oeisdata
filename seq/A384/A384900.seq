%e A384900   0,  4, 18, 48, 101, 185, ...
%e A384900   0, -3, 14, 72, 200, 436, ...
%o A384900 (PARI) b(n, k) = if(n*k==0, 0^n, (-1)^n*k*sum(j=1, n, binomial(-2*n+3*j+k-1,j-1)*b(n-j,2*j)/j));
%o A384900 a(n, k) = b(n, -k);
