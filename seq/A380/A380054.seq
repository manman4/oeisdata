%o A380054 (PARI) /* From definition A( A(x)^3 - A(x)^4 ) = x*A(x)^2 */
%o A380054 {a(n) = my(A=[1, 1]); for(i=1, n, A = concat(A, 0); G=x*Ser(A); A[#A] = -Vec( subst(G, x, G^3 - G^4 )/G )[#A] ); A[n]}
%o A380054 for(n=1, 40, print1(a(n), ", "))
%o A380054 (PARI) /* Using Series Reversion of A(x^3-x^4)/x^2 */
%o A380054 {a(n) = my(A=x); for(i=1, n, A = serreverse( subst(A, x, x^3 - x^4 +x^3*O(x^n))/x^2) ); polcoeff(A, n)}
%o A380054 for(n=1, 40, print1(a(n), ", "))
