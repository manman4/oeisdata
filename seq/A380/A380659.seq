%t A380659 f[{x_, y_}] := If[Mod[x, 4] == 1, y, -y];
%t A380659 s[n_] := Map[f, FactorInteger[n]];
%t A380659 p[n_] := {Total[Select[s[n], # > 0 &]], -Total[Select[s[n], # < 0 &]]};
%t A380659 t = Table[p[n], {n, 1, 200000}];
%t A380659 u = Map[Last, t];
%t A380659 row[n_] := Flatten[Position[u, n]]
%t A380659 v[n_] := Take[row[n], 12]
%t A380659 Column[Table[v[n], {n, 0, 12}]]
%t A380659 Grid[Table[v[n], {n, 0, 12}]]
%t A380659 w[m_, n_] := row[m][[n]];
%t A380659 Grid[Table[w[m, n], {m, 0, 12}, {n, 1, 12}]]   (* array *)
%t A380659 Table[w[n - k, k], {n, 0, 12}, {k, n, 1, -1}] // Flatten  (* sequence *)
