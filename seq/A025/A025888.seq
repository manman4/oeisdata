%o A025888 (SageMath)
%o A025888 def A025888_list(prec):
%o A025888     P.<x> = PowerSeriesRing(ZZ, prec)
%o A025888     return P( 1/((1-x^5)*(1-x^8)*(1-x^10)) ).list()
%o A025888 A025888_list(90) # _G. C. Greubel_, Nov 19 2022
