%t A064423 terms = 100;
%t A064423 ekg[s_] := Block[{m = s[[-1]], k = 3}, While[MemberQ[s, k] || GCD[m, k] == 1, k++]; Append[s, k]];
%t A064423 EKG = Nest[ekg, {2, 4}, 12 terms];
%t A064423 a[n_] := FirstPosition[EKG, Prime[n]][[1]];
