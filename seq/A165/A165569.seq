%o A165569 (define (A165569 n) (if (= 1 n) 1 (let* ((i (A165569 (-1+ n))) (champion (abs (- *phi* (/ (A108539 i) (A000040 i)))))) (let loop ((i (1+ i))) (cond ((< (abs (- *phi* (/ (A108539 i) (A000040 i)))) champion) i) (else (loop (1+ i))))))))
%o A165569 (define *phi* (/ (1+ (sqrt 5)) 2))
