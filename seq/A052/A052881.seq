%p A052881 A052881 := n -> `if`(n<2,0,2*(n-1)*(n-1)!*hypergeom([1,1,2-n],[2,n+1],-1)):
%p A052881 seq(simplify(A052881(n)),n=0..19); # _Peter Luschny_, Jun 11 2016
