%o A052692 def A003269(n):
%o A052692     if (n<4): return 1-bool(n==0)
%o A052692     else: return A003269(n-1) + A003269(n-4)
%o A052692 def A052692(n): return factorial(n)*A003269(n) +bool(n==0)
