%F A015467 a(n) = a(n-1) + 9^(n-2) * a(n-2).
%p A015467 q:=9; seq(add((product((1-q^(n-j-1-k))/(1-q^(k+1)), k=0..j-1))*q^(j^2), j = 0..floor((n-1)/2)), n = 0..20); # _G. C. Greubel_, Dec 16 2019
