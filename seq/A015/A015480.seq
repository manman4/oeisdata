%N A015480 q-Fibonacci numbers for q=8, scaling a(n-1).
%H A015480 Vincenzo Librandi, <a href="/A015480/b015480.txt">Table of n, a(n) for n = 0..40</a>
%F A015480 a(n) = 8^(n-1)*a(n-1) + a(n-2).
%p A015480 q:=8; seq(add((product((1-q^(2*(n-j-1-k)))/(1-q^(2*k+2)), k=0..j-1))* q^binomial(n-2*j,2), j = 0..floor((n-1)/2)), n = 0..20); # _G. C. Greubel_, Dec 18 2019
%t A015480 RecurrenceTable[{a[0]==0, a[1]==1, a[n]==a[n-1]*8^(n-1)+a[n-2]}, a, {n, 20}] (* _Vincenzo Librandi_, Nov 10 2012 *)
%t A015480 F[n_, q_]:= Sum[QBinomial[n-j-1, j, q^2]*q^Binomial[n-2*j,2], {j, 0, Floor[(n-1)/2]}]; Table[F[n, 8], {n, 0, 20}] (* _G. C. Greubel_, Dec 18 2019 *)
%o A015480 (PARI) q=8; m=20; v=concat([0,1], vector(m-2)); for(n=3, m, v[n]=q^(n-2)*v[n-1]+v[n-2]); v \\ _G. C. Greubel_, Dec 18 2019
%o A015480 (Magma) q:=8; I:=[0,1]; [n le 2 select I[n] else q^(n-2)*Self(n-1) + Self(n-2): n in [1..20]]; // _G. C. Greubel_, Dec 18 2019
%o A015480 (SageMath)
%o A015480 def F(n,q): return sum( q_binomial(n-j-1, j, q^2)*q^binomial(n-2*j,2) for j in (0..floor((n-1)/2)))
%o A015480 [F(n,8) for n in (0..20)] # _G. C. Greubel_, Dec 18 2019
%o A015480 (GAP) q:=8;; a:=[0,1];; for n in [3..20] do a[n]:=q^(n-2)*a[n-1]+a[n-2]; od; a; # _G. C. Greubel_, Dec 18 2019
