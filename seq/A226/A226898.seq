%o A226898 (Python)
%o A226898 from sympy import divisors, exp
%o A226898 def a(n):
%o A226898     d = divisors(n)
%o A226898     m = 1
%o A226898     for i in range(len(d) - 1):
%o A226898         t = exp(1)*d[i]
%o A226898         m = max(sum(1 for j in range(i, len(d)) if d[j]<t), m)
%o A226898     return m
