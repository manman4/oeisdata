%o A319572 (MATLAB)
%o A319572 function a=A319572(nmax)
%o A319572 a=zeros(1,nmax);
%o A319572 for n=0:nmax
%o A319572     b=floor( sqrt(2*(n+1))+0.5); % A002024(n+1)
%o A319572     c=(-1)^b;
%o A319572     a(n+1)=[(1-c)/2]*(b-1) + c*[ (n+1)-ceil( ((b-0.5)^2)/2 ) ];
%o A319572 end
