%t A141697 i=12; l=14; Table[Table[(l*Sum[(-1)^j*Binomial[n+1, j](k+1-j)^n, {j, 0, k+1}] - i*Binomial[n-1, k])/2, {k,0,n-1}], {n,10}]//Flatten
