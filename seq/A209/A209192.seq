%p A209192 A010888 := proc(n)
%p A209192     d := (add(d,d=convert(n,base,10))) mod 9 ;
%p A209192     if d = 0 then
%p A209192         return 9;
%p A209192     else
%p A209192         return d;
%p A209192     end if;
%p A209192 end proc:
%p A209192 isA209192 := proc(n)
%p A209192     A010888(n) = A006530(n) ;
%p A209192 end proc:
