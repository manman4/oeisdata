%o A093829 (PARI) {a(n) = local(A); if( n<1, 0, n--; A = x * O(x^n); polcoeff( eta(x + A) * eta(x^6 + A)^6 / (eta(x^2 + A)^2 * eta(x^3 + A)^3), n))};
