%o A093995 a093995_tabl = zipWith replicate [1..] $ tail a000290_list
%o A093995 a093995_list = concat a093995_tabl
%o A093995 -- _Reinhard Zumkeller_, Nov 11 2012, Mar 18 2011, Oct 17 2010
