%t A133607 qStirling2[n_, k_, q_] /; 1 <= k <= n := q^(k-1) qStirling2[n-1, k-1, q] + Sum[q^j, {j, 0, k-1}] qStirling2[n-1, k, q];
%t A133607 qStirling2[n_, 0, _] := KroneckerDelta[n, 0];
%t A133607 qStirling2[0, k_, _] := KroneckerDelta[0, k];
%t A133607 qStirling2[_, _, _] = 0;
