%p A158454 A158454 := proc(n,k) (-1)^(n+k)*add(binomial(n+k-1-2*j,2*k-1),j=0..floor(n/2)) ; end proc;
%p A158454 seq(seq(A158454(n,k),k=0..n),n=0..10) ; # _R. J. Mathar_, Dec 17 2010
