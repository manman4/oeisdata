%t A035469 rows = 9;
%t A035469 a[n_, m_] := BellY[n, m, Table[Product[3k+1, {k, 0, j}], {j, 0, rows}]];
%t A035469 Table[a[n, m], {n, 1, rows}, {m, 1, n}] // Flatten (* _Jean-François Alcover_, Jun 22 2018 *)
