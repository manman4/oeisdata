%o A389648 (Python)
%o A389648 def traverse_upper_halfplane(n):
%o A389648     points = [(0, 0)]
%o A389648     arch = 1
%o A389648     sign = 1
%o A389648     while len(points) < n:
%o A389648         for y in range(arch+1):
%o A389648             points.append((sign*arch, y))
%o A389648         for x in range(arch-1, -arch-1, -1):
%o A389648             points.append((sign*x, arch))
%o A389648         for y in range(arch-1, -1, -1):
%o A389648             points.append((-sign*arch, y))
%o A389648         arch += 1
%o A389648         sign = - sign
%o A389648     return points[:n]
%o A389648 l = [y for (x, y) in traverse_upper_halfplane(106)]
