%t A389577 highs := {Map[First, #], Most[FoldList[Plus, 1, Map[Length, #]]]} &[
%t A389577 Split[Rest[FoldList[Max, -\[Infinity], #]]]] &;
%t A389577 lows := {Map[First, #], Most[FoldList[Plus, 1, Map[Length, #]]]} &[
%t A389577 Split[Rest[FoldList[Min, +\[Infinity], #]]]] &;
%t A389577 c = N[ChampernowneNumber[2], 500];
%t A389577 r[n_] := FractionalPart[c*2^n];
%t A389577 seqS = Table[r[n], {n, 0, 480}];  (* User: put your dense sequence S after seqS *)
%t A389577 indices = Range[Length[seqS]];
%t A389577 arrI = {};   (* start accumulating increasing partition array *)
%t A389577 Until[Last[arrI] == {}, AppendTo[arrI, Flatten[Map[Position[seqS, #] &,
%t A389577 highs[seqS[[Complement[indices, Flatten[arrI]]]]][[1]]]]]];
%t A389577 Grid[Take[arrI, 12]]
%t A389577 arrD = {};   (* start accumulating decreasing partition array *)
%t A389577 Until[Last[arrD] == {}, AppendTo[arrD, Flatten[Map[Position[seqS, #] &,
%t A389577 lows[seqS[[Complement[indices, Flatten[arrD]]]]][[1]]]]]];
%t A389577 Grid[Take[arrD, 12]]
