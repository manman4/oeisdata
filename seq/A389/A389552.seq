%p A389552 a:= n-> LinearAlgebra[Determinant]((l-> Matrix(nops(l), (i, j)
%p A389552         ->(l[i])^(j-1)))(sort([numtheory[divisors](n)[]]))):
%p A389552 seq(a(n), n=1..35);  # _Alois P. Heinz_, Oct 09 2025
%t A389552 a[n_]:=Det[Table[Part[Divisors[n],i]^(j-1),{i,DivisorSigma[0,n]},{j,DivisorSigma[0,n]}]]; Array[a,35] (* _Stefano Spezia_, Oct 08 2025 *)
%o A389552 (PARI) a(n) = my(d=divisors(n)); matdet(matrix(#d, #d, i, j, d[i]^(j-1)));
