%o A389168 (PARI)
%o A389168 up_to_cols = 210;
%o A389168 A003961(n) = { my(f = factor(n)); for (i=1, #f~, f[i, 1] = nextprime(f[i, 1]+1)); factorback(f); };
%o A389168 A388984list(up_to) = { my(v=vector(up_to), i=0); forstep(n=2,oo,2,if(isprime(core(n)) && !isprimepower(n), i++; v[i] = n; if(i==up_to, return(v)))); };
%o A389168 v388984 = A388984list(up_to_cols);
%o A389168 A388984(n) = v388984[n];
%o A389168 up_to = binomial(up_to_cols+1,2)
%o A389168 A388981sq(row,col) = if(1==row,A388984(col), my(f = factor(A388984(col))); for(i=1, #f~, f[i,1] = prime(primepi(f[i,1])+(row-1))); factorback(f));
%o A389168 A388978(n) = { my(u=A003961(n)); ((n-core(n))*(sigma(u)-core(u)-u)); };
%o A389168 A389168sq(row,col) = A388978(A388981sq(row,col));
%o A389168 A389168list(up_to) = { my(v = vector(up_to), i=0); for(a=1,oo, for(col=1,a, i++; if(i > up_to, return(v)); v[i] = A389168sq(col,(a-(col-1))))); (v); };
%o A389168 v389168 = A389168list(up_to);
%o A389168 A389168(n) = v389168[n];
