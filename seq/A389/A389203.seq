%t A389203 nn = 100; n = 1; t = {}; While[Length[t] < nn, n = n + 2; {p, e} = Transpose[FactorInteger[n]]; od = Select[e, OddQ]; If[Length[e] > 1 && Length[od] == 1 && Mod[od[[1]], 4] == 1 && Mod[p[[Position[e, od[[1]]][[1, 1]]]], 4] == 1, AppendTo[t, n]]];DivisorSigma[1,t] (* _James C. McMahon_, Sep 30 2025 *)
%o A389203 (PARI)
%o A389203 up_to = 20000;
%o A389203 isA228058(n) = if(!(n%2)||(omega(n)<2), 0, my(f=factor(n), y=0); for(i=1, #f~, if(1==(f[i, 2]%4), if((1==y)||(1!=(f[i, 1]%4)), return(0), y=1), if(f[i, 2]%2, return(0)))); (y));
%o A389203 A228058list(up_to) = { my(v=vector(up_to), k=0, n=0); while(k<up_to, n++; if(isA228058(n), k++; v[k] = n)); (v); };
%o A389203 v228058 = A228058list(up_to);
%o A389203 A228058(n) = v228058[n];
%o A389203 A389203(n) = sigma(A228058(n));
