%o A389383 (Python)
%o A389383 import math
%o A389383 def row(n):
%o A389383     if n==1: return [1]
%o A389383     l=[n]
%o A389383     while True:
%o A389383         if n%2: n=math.isqrt(n**3)
%o A389383         else:   n=math.isqrt(n)
%o A389383         if n not in l:
%o A389383             l+=[n]
%o A389383             if n==1: break
%o A389383         else:
%o A389383             break
%o A389383     return l
%o A389383 for n in range(1,101): print(row(n))
