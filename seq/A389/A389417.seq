%t A389417 Select[Prime[Range[100]], Catch[With[{d = IntegerDigits[#]}, Do[If[d[[s]] > 0 && AnyTrue[Range[s, Length[d]-1], PrimeQ[FromDigits[Join[d[[;; s-1]], IntegerDigits[FromDigits[d[[s ;; #]]]*2], d[[#+1 ;; ]]]]] &], Throw[True]], {s, Length[d]-1}]; False]] &]
%o A389417 (Python)
%o A389417 from sympy import isprime
%o A389417 def ok(n):
%o A389417     if n%10 not in {1, 3, 7, 9} or not isprime(n): return False
%o A389417     s = str(n)
%o A389417     return any(isprime(int(s[:i]+str(2*int(s[i:j]))+s[j:])) for i in range(len(s)) for j in range(i+1, len(s)+1) if s[i]!='0')
%o A389417 print([k for k in range(440) if ok(k)]) # _Michael S. Branicky_, Oct 03 2025
