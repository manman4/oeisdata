%o A389512 (Python)
%o A389512 from itertools import groupby
%o A389512 from sympy.combinatorics.partitions import IntegerPartition
%o A389512 from sympy.utilities.iterables import partitions, multiset_permutations
%o A389512 def cond(t): return all(len(list(g))<k for k, g in groupby(t))
%o A389512 def a(n): return sum(1 for p in partitions(n) if 1 not in p for c in multiset_permutations(IntegerPartition(p).partition) if cond(c))
%o A389512 print([a(n) for n in range(30)]) # _Michael S. Branicky_, Oct 11 2025
