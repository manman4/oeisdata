%N A389299 a(n) is the least number that has exactly n exponential abundant divisors.
%C A389299 36*5^k has k-1 exponential abundant divisors for k >= 1. Therefore, a(n) exists for all n.
%t A389299 f[p_, e_] := DivisorSum[e, p^# &]; esigma[1] = 1; esigma[n_] := Times @@ f @@@ FactorInteger[n];
%t A389299 s[n_] := Count[Divisors[n], _?(esigma[#] > 2*# &)];
%t A389299 seq[len_] := Module[{v = Table[0, {len}], c = 0, n = 1, i}, While[c < len, i = s[n] + 1; If[i <= len && v[[i]] == 0, c++; v[[i]] = n]; n++]; v]; seq[14]
%o A389299 (PARI) esigma(n) = {my(f = factor(n)); prod(k=1, #f~, sumdiv(f[k, 2], d, f[k, 1]^d));}
%o A389299 s(n) = sumdiv(n, d, esigma(d) > 2*d);
%o A389299 list(len) = {my(v = vector(len), c = 0, n = 1, i); while(c < len, i = s(n) + 1; if(i <= len && v[i] == 0, c++; v[i] = n); n++); v;}
