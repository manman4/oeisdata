%t A389576 highs := {Map[First, #], Most[FoldList[Plus, 1, Map[Length, #]]]} &[
%t A389576 Split[Rest[FoldList[Max, -\[Infinity], #]]]] &;
%t A389576 lows := {Map[First, #], Most[FoldList[Plus, 1, Map[Length, #]]]} &[
%t A389576 Split[Rest[FoldList[Min, +\[Infinity], #]]]] &;
%t A389576 c = N[ChampernowneNumber[2], 500];
%t A389576 r[n_] := FractionalPart[c*2^n];
%t A389576 seqS = Table[r[n], {n, 0, 480}];  (* User: put your dense sequence S after seqS *)
%t A389576 indices = Range[Length[seqS]];
%t A389576 arrI = {};   (* start accumulating increasing partition array *)
%t A389576 Until[Last[arrI] == {}, AppendTo[arrI, Flatten[Map[Position[seqS, #] &,
%t A389576 highs[seqS[[Complement[indices, Flatten[arrI]]]]][[1]]]]]];
%t A389576 Grid[Take[arrI, 12]]
%t A389576 arrD = {};   (* start accumulating decreasing partition array *)
%t A389576 Until[Last[arrD] == {}, AppendTo[arrD, Flatten[Map[Position[seqS, #] &,
%t A389576 lows[seqS[[Complement[indices, Flatten[arrD]]]]][[1]]]]]];
%t A389576 Grid[Take[arrD, 12]]
