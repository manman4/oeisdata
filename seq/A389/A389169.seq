%o A389169 (PARI)
%o A389169 up_to_cols = 210;
%o A389169 A003961(n) = { my(f = factor(n)); for (i=1, #f~, f[i, 1] = nextprime(f[i, 1]+1)); factorback(f); };
%o A389169 A388984list(up_to) = { my(v=vector(up_to), i=0); forstep(n=2,oo,2,if(isprime(core(n)) && !isprimepower(n), i++; v[i] = n; if(i==up_to, return(v)))); };
%o A389169 v388984 = A388984list(up_to_cols);
%o A389169 A388984(n) = v388984[n];
%o A389169 up_to = binomial(up_to_cols+1,2)
%o A389169 A388981sq(row,col) = if(1==row,A388984(col), my(f = factor(A388984(col))); for(i=1, #f~, f[i,1] = prime(primepi(f[i,1])+(row-1))); factorback(f));
%o A389169 A388979(n) = ((sigma(n)-core(n)-n)*(A003961(n)-A003961(core(n))));
%o A389169 A389169sq(row,col) = A388979(A388981sq(row,col));
%o A389169 A389169list(up_to) = { my(v = vector(up_to), i=0); for(a=1,oo, for(col=1,a, i++; if(i > up_to, return(v)); v[i] = A389169sq(col,(a-(col-1))))); (v); };
%o A389169 v389169 = A389169list(up_to);
%o A389169 A389169(n) = v389169[n];
