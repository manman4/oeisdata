%t A389574 highs := {Map[First, #], Most[FoldList[Plus, 1, Map[Length, #]]]} &[
%t A389574 Split[Rest[FoldList[Max, -\[Infinity], #]]]] &;
%t A389574 lows := {Map[First, #], Most[FoldList[Plus, 1, Map[Length, #]]]} &[
%t A389574 Split[Rest[FoldList[Min, +\[Infinity], #]]]] &;
%t A389574 c = N[ChampernowneNumber[10], 50000];
%t A389574 r[n_] := FractionalPart[c*10^n];
%t A389574 seqS = Table[r[n], {n, 0, 48000}];  (* User: put your dense sequence S after seqS *)
%t A389574 indices = Range[Length[seqS]];
%t A389574 arrI = {};   (* start accumulating increasing partition array *)
%t A389574 Until[Last[arrI] == {}, AppendTo[arrI, Flatten[Map[Position[seqS, #] &,
%t A389574 highs[seqS[[Complement[indices, Flatten[arrI]]]]][[1]]]]]];
%t A389574 Grid[Take[arrI, 12]]
%t A389574 arrD = {};   (* start accumulating decreasing partition array *)
%t A389574 Until[Last[arrD] == {}, AppendTo[arrD, Flatten[Map[Position[seqS, #] &,
%t A389574 lows[seqS[[Complement[indices, Flatten[arrD]]]]][[1]]]]]];
%t A389574 Grid[Take[arrD, 12]]
