%t A389922 stc[n_]:=Differences[Prepend[Join @@ Position[Reverse[IntegerDigits[n,2]],1],0]]//Reverse;
%t A389922 Select[Range[0,1000],Min[stc[#]]==Length[stc[#]]&]
