%F A389096 a(n) = A325378(n) + A389164(n).
%o A389096 (PARI)
%o A389096 up_to = 20410;
%o A389096 isA228058(n) = if(!(n%2)||(omega(n)<2), 0, my(f=factor(n), y=0); for(i=1, #f~, if(1==(f[i, 2]%4), if((1==y)||(1!=(f[i, 1]%4)), return(0), y=1), if(f[i, 2]%2, return(0)))); (y));
%o A389096 A228058list(up_to) = { my(v=vector(up_to), k=0, n=0); while(k<up_to, n++; if(isA228058(n), k++; v[k] = n)); (v); };
%o A389096 v228058 = A228058list(up_to);
%o A389096 A228058(n) = v228058[n];
%o A389096 A162296(n) = sumdiv(n, d, d*(1-issquarefree(d)));
%o A389096 A389096(n) = A162296(A228058(n));
