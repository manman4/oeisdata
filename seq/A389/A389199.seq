%t A389199 A389199Q[k_] := k > 1 && Mod[Times @@ (FactorInteger[k][[All, 1]] + 1), 4] == 2;
%t A389199 Select[Range[500], A389199Q] (* _Paolo Xausa_, Oct 13 2025 *)
%o A389199 (PARI)
%o A389199 A048250(n) = factorback(apply(p -> p+1,factor(n)[,1]));
%o A389199 is_A389199(k) = (2==(A048250(k)%4));
%o A389199 (PARI)
%o A389199 A000265(n) = (n>>valuation(n,2));
