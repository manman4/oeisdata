%N A389899 Lexicographically earliest infinite sequence such that a(i) = a(j) => A343029(A126760(i)) = A343029(A126760(j)) and A343030(A126760(i)) = A343030(A126760(j)) for all i, j >= 0.
%C A389899 Restricted growth sequence transform of the ordered pair [A343029(A126760(n)), A343030(A126760(n))].
%H A389899 Antti Karttunen, <a href="/A389899/b389899.txt">Table of n, a(n) for n = 0..65537</a>
%F A389899 a(n) = A361020(A126760(n)).
%o A389899 (PARI)
%o A389899 up_to = 65537;
%o A389899 rgs_transform(invec) = { my(om = Map(), outvec = vector(length(invec)), u=1); for(i=1, length(invec), if(mapisdefined(om,invec[i]), my(pp = mapget(om, invec[i])); outvec[i] = outvec[pp] , mapput(om,invec[i],i); outvec[i] = u; u++ )); outvec; };
%o A389899 A126760(n) = {n && n\=3^valuation(n, 3)<<valuation(n, 2); n%3+n\6*2 };
%o A389899 A343029(n) = { my(t=1, ret=0); for(i=0, if(n, logint(n, 2)), if(bittest(n, i), ret+=t, t=!t)); ret; };
%o A389899 A343030(n) = { my(t=0, ret=0); for(i=0, if(n, logint(n, 2)), if(bittest(n, i), ret+=t, t=!t)); ret; };
%o A389899 Aux361020(n) = [A343029(n), A343030(n)];
%o A389899 v361020 = rgs_transform(vector(1+up_to,n,Aux361020(n-1)));
%o A389899 A361020(n) = v361020[1+n];
%o A389899 A389899(n) = A361020(A126760(n));
