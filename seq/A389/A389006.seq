%o A389006 (Python)
%o A389006 from itertools import count, islice, product
%o A389006 def pals(): # generator of palindromes
%o A389006     digits = "".join(str(i) for i in range(10))
%o A389006     for d in count(1):
%o A389006         for p in product(digits, repeat=d//2):
%o A389006             if d > 1 and p[0] == "0": continue
%o A389006             left = "".join(p); right = left[::-1]
%o A389006             for mid in [[""], digits][d%2]: yield int(left + mid + right)
%o A389006 def agen(): # generator of terms
%o A389006     a3, sumset = [0, 0, 0], {0}; yield 0
%o A389006     while True:
%o A389006         m = sum(a3[1:])
%o A389006         an, s = next((p, m+p) for p in pals() if m+p not in sumset)
%o A389006         yield s; a3 = a3[1:] + [an]; sumset |= {s}
%o A389006 print(list(islice(agen(), 68))) # _Michael S. Branicky_, Sep 26 2025
