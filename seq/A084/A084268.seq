%o A084268 for n in range(1, 8) :
%o A084268     st = [0 for j in range(n+1)]
%o A084268     G = graphs(n)
%o A084268     for g in G :
%o A084268         st[ g.chromatic_number() ] += 1
%o A084268     print(st)
