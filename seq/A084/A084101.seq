%e A084101 (End)
%t A084101 CoefficientList[Series[(1+x)^2/((1-x)(1+x^2)),{x,0,110}],x] (* or *) PadRight[{},110,{1,3,3,1}] (* _Harvey P. Dale_, Nov 21 2012 *)
