%o A379199 (PARI) {a(n) = my(V=[0, 1], A); for(i=1, n, V=concat(V, 0); A = Ser(V);
%o A379199 V[#V] = polcoef( sum(m=-#A, #A, A^m*(A^m - 1)^(m+1) ), #V-3); ); polcoef(A, n)}
