%o A388272 is_A388271(n) = { my(s=sigma(n), p=2); if(s%2, return(0), s /= 2); while(n, if(min(n%p, s%p) != (n%p), return(0)); n = n\p; s = s\p; p = nextprime(1+p)); (1); };
%o A388272 is_A388272(n) = if(!is_A388271(n), 0, fordiv(n, d, if(d<n && is_A388271(d), return(0))); (1));
