%o A388982 (PARI)
%o A388982 up_to_cols = 210;
%o A388982 A388980list(up_to) = { my(v=vector(up_to), i=0); forstep(n=2,oo,2,if(isprime(core(n)), i++; v[i] = n; if(i==up_to, return(v)))); };
%o A388982 v388980 = A388980list(up_to_cols);
%o A388982 A388980(n) = v388980[n];
%o A388982 up_to = binomial(up_to_cols+1,2)
%o A388982 A388982sq(row,col) = if(1==row,A388980(col), my(f = factor(A388980(col))); for(i=1, #f~, f[i,1] = prime(primepi(f[i,1])+(row-1))); factorback(f));
%o A388982 A388982list(up_to) = { my(v = vector(up_to), i=0); for(a=1,oo, for(col=1,a, i++; if(i > up_to, return(v)); v[i] = A388982sq(col,(a-(col-1))))); (v); };
%o A388982 v388982 = A388982list(up_to);
%o A388982 A388982(n) = v388982[n];
