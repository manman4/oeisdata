%N A388845 The smallest start of a run of exactly n consecutive positive integers for which the number of exponential divisors of their factorials is strictly increasing.
%t A388845 d[n_] := d[n] = DivisorSigma[0, n]; d[0] = 1;
%t A388845 r[n_] := Product[d[(n - DigitSum[n, p])/(p - 1)] / d[(n - 1 - DigitSum[n - 1, p])/(p - 1)], {p, FactorInteger[n][[;; , 1]]}]; r[1] = 1;
%t A388845 seq[len_] := Module[{v = Table[0, {len}], nsuc = 0, c = 0, k = 1, m}, While[c < len, m = nsuc; If[r[k] > 1, nsuc++, If[k > m && m <= len && v[[m]] == 0, v[[m]] = k - m; c++]; nsuc = 1]; k++]; v]; seq[12]
%o A388845 (PARI) d(n) = if(n == 0, 1, numdiv(n));
%o A388845 r(n) = {my(p = factor(n)[,1]); prod(i = 1, #p, d((n - sumdigits(n, p[i]))/(p[i] - 1)) / d((n - 1 - sumdigits(n - 1, p[i]))/(p[i] - 1)));}
%o A388845 list(len) = {my(v = vector(len), nsuc = 0, c = 0, k = 1, m); while(c < len, m = nsuc; if(r(k) > 1, nsuc++, if(k > m && m > 0 && m <= len && v[m] == 0, v[m] = k-m; c++); nsuc = 1); k++); v;}
