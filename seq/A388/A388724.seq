%t A388724 suppstrQ[mset_]:=And@@Table[mset[[i]]>i,{i,Length[mset]}];
%t A388724 Table[Length[Select[Reverse/@IntegerPartitions[n],Max@@#==k&&suppstrQ[#]&]],{n,0,15},{k,0,n}]
%o A388724 (PARI)
%o A388724 tri(n) = {(n*(n+1)/2)}
%o A388724 b(n) = {floor(1/2+sqrt(2*n))}
%o A388724 C(q, n) = {c = if(n<=1, return(1)); return(sum(i=0, n-1, C(q, i) * C(q, n-1-i) * q^((i+1)*(n-1 -i))));}
%o A388724 Ca(n) = {polrecip(C(q,n))}
%o A388724 A388724_q(rowmax) = {my(N = rowmax+1,pl= vector(b(N),i,1)); for(m=1,b(N), my(g = sum(j=1,m, q^(tri(j)+j*(m-j)+j-1) * t^j *Ca(j-1) * if(j<m,pl[m-j]/(1-t*q^(m-j)),1))); pl[m]=g); my(f = sum(m=1,b(N), t*q^m* pl[m]/(1-t*q^m))  + O('q^(N+1))); vector(N, n, if(n==2,[0,0],Vecrev(polcoeff(f, n-1))))} \\ _John Tyler Rascoe_, Oct 04 2025
