%t A388856 highs := {Map[First, #], Most[FoldList[Plus, 1, Map[Length, #]]]} &[Split[Rest[FoldList[Max, -\[Infinity], #]]]] &;
%t A388856 lows := {Map[First, #], Most[FoldList[Plus, 1, Map[Length, #]]]} &[Split[Rest[FoldList[Min, +\[Infinity], #]]]] &;
%t A388856 seqS = Table[N[Sin[n], 20], {n, 1, 4000}];  (* User: put your dense sequence S after seqS *)
%t A388856 indices = Range[Length[seqS]];
%t A388856 arrI = {};   (* start accumulating increasing partition array *)
%t A388856 Until[Last[arrI] == {}, AppendTo[arrI,  Flatten[Map[Position[seqS, #] &,
%t A388856      highs[seqS[[Complement[indices, Flatten[arrI]]]]][[1]]]]]];
%t A388856 Grid[Take[arrI, 12]]
%t A388856 arrD = {};   (* start accumulating decreasing partition array *)
%t A388856 Until[Last[arrD] == {}, AppendTo[arrD,
%t A388856    Flatten[Map[Position[seqS, #] &, lows[seqS[[Complement[indices, Flatten[arrD]]]]][[1]]]]]];
%t A388856 Grid[Take[arrD, 12]]
