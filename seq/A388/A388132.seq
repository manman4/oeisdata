%N A388132 Triangular array read by rows: row n consists of the numbers k such that A075255(k) = A385964(n).
%H A388132 Robert Israel, <a href="/A388132/b388132.txt">Table of n, a(n) for n = 1..231</a>
%e A388132 Row 3 is 20, 21, 26 because A385964(3) = 11 and the 3 numbers k such that A075255(k) = 11 are 20, 21, 26 (11 = 20 - 2*2 - 5 = 21 - 3 - 7 = 26 - 2 - 13.
%e A388132 The first 8 rows are
%e A388132  8;
%e A388132  1, 6;
%e A388132  20, 21, 26;
%e A388132  49, 52, 57, 74;
%e A388132  88, 91, 95, 111, 146;
%e A388132  810, 846, 851, 871, 995, 1191;
%e A388132  2117, 2147, 2159, 2359, 2692, 3027, 4034;
%e A388132  1280, 1302, 1333, 1349, 1397, 1477, 1684, 1893.
%p A388132 sopfr:= proc(n) local F, t;
%p A388132   F:= ifactors(n)[2];
%p A388132   add(t[1]*t[2], t=F)
%p A388132 end proc:
%p A388132 M:= 5*10^6: Mh:= M/2-2:
%p A388132 R:= map(t -> t - sopfr(t) , [$1..M]):
%p A388132 RS:= sort(select(t -> t > 0 and t <= Mh, R)):
%p A388132 V:= Array(1..21):
%p A388132 state:= 1: r:= RS[1]:
%p A388132 for i from 2 to nops(RS) do
%p A388132   if RS[i] = r then
%p A388132     state:= state+1
%p A388132   else
%p A388132     if V[state] = 0 then V[state]:= r fi;
%p A388132     state:= 1: r:= RS[i]
%p A388132   fi
%p A388132 od:
%p A388132 VL:=convert(V,list);
%p A388132 T:= Vector(22):
%p A388132 for i from 1 to M do
%p A388132   if member(R[i],VL,'k') then if T[k] = 0 then T[k]:= i else T[k]:= T[k],i fi fi
%p A388132 od:
%p A388132 for i from 1 to 21 do T[i] od;
