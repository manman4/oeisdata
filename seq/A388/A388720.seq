%e A388720                                             11222     111222
%e A388720                                             111122    1111122
%e A388720                                             11111111  111111111
%t A388720 suppQ[mset_]:=And@@Table[mset[[i]]>=i,{i,Length[mset]}];
%t A388720 Table[Length[Select[Reverse/@IntegerPartitions[n],suppQ[Length/@Split[#]]&]],{n,0,10}]
