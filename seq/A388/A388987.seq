%o A388987 (PARI)
%o A388987 up_to_cols = 210;
%o A388987 A326127(n) = (sigma(n)-core(n)-n);
%o A388987 A388984list(up_to) = { my(v=vector(up_to), i=0); forstep(n=2,oo,2,if(isprime(core(n)) && !isprimepower(n), i++; v[i] = n; if(i==up_to, return(v)))); };
