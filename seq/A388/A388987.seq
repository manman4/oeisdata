%o A388987 A388981sq(row,col) = if(1==row,A388984(col), my(f = factor(A388984(col))); for(i=1, #f~, f[i,1] = prime(primepi(f[i,1])+(row-1))); factorback(f));
%o A388987 A388987sq(row,col) = A326127(A388981sq(row,col));
%o A388987 A388987list(up_to) = { my(v = vector(up_to), i=0); for(a=1,oo, for(col=1,a, i++; if(i > up_to, return(v)); v[i] = A388987sq(col,(a-(col-1))))); (v); };
%o A388987 v388987 = A388987list(up_to);
