%t A388853 highs := {Map[First, #], Most[FoldList[Plus, 1, Map[Length, #]]]} &[Split[Rest[FoldList[Max, -\[Infinity], #]]]] &;
%t A388853 lows := {Map[First, #], Most[FoldList[Plus, 1, Map[Length, #]]]} &[Split[Rest[FoldList[Min, +\[Infinity], #]]]] &;
%t A388853 seqS = Rest[Rest[SortBy[FareySequence[40], Denominator[#] &]]];
%t A388853 (* User: put your dense sequence S just above, after seqS = *)
%t A388853 indices = Range[Length[seqS]];
%t A388853 arrI = {};   (* start accumulating increasing partition array *)
%t A388853 Until[Last[arrI] == {}, AppendTo[arrI,  Flatten[Map[Position[seqS, #] &,
%t A388853      highs[seqS[[Complement[indices, Flatten[arrI]]]]][[1]]]]]];
%t A388853 Grid[Most[arrI], Frame -> All]
%t A388853 arrD = {};   (* start accumulating decreasing partition array *)
%t A388853 Until[Last[arrD] == {}, AppendTo[arrD,
%t A388853    Flatten[Map[Position[seqS, #] &, lows[seqS[[Complement[indices, Flatten[arrD]]]]][[1]]]]]];
%t A388853 Grid[Most[arrD], Frame -> All]
