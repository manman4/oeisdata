%o A388988 (PARI)
%o A388988 up_to_cols = 210;
%o A388988 A326128(n) = (n-core(n));
%o A388988 A388984list(up_to) = { my(v=vector(up_to), i=0); forstep(n=2,oo,2,if(isprime(core(n)) && !isprimepower(n), i++; v[i] = n; if(i==up_to, return(v)))); };
