%o A388997 (PARI)
%o A388997 up_to = 20000;
%o A388997 isA228058(n) = if(!(n%2)||(omega(n)<2), 0, my(f=factor(n), y=0); for(i=1, #f~, if(1==(f[i, 2]%4), if((1==y)||(1!=(f[i, 1]%4)), return(0), y=1), if(f[i, 2]%2, return(0)))); (y));
%o A388997 A228058list(up_to) = { my(v=vector(up_to), k=0, n=0); while(k<up_to, n++; if(isA228058(n), k++; v[k] = n)); (v); };
%o A388997 v228058 = A228058list(up_to);
%o A388997 A228058(n) = v228058[n];
%o A388997 A003557(n) = (n/factorback(factor(n)[, 1]));
