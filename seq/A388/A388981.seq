%o A388981 (PARI)
%o A388981 up_to_cols = 210;
%o A388981 A388984list(up_to) = { my(v=vector(up_to), i=0); forstep(n=2,oo,2,if(isprime(core(n)) && !isprimepower(n), i++; v[i] = n; if(i==up_to, return(v)))); };
%o A388981 v388984 = A388984list(up_to_cols);
%o A388981 A388984(n) = v388984[n];
%o A388981 up_to = binomial(up_to_cols+1,2)
%o A388981 A388981sq(row,col) = if(1==row,A388984(col), my(f = factor(A388984(col))); for(i=1, #f~, f[i,1] = prime(primepi(f[i,1])+(row-1))); factorback(f));
%o A388981 A388981list(up_to) = { my(v = vector(up_to), i=0); for(a=1,oo, for(col=1,a, i++; if(i > up_to, return(v)); v[i] = A388981sq(col,(a-(col-1))))); (v); };
%o A388981 v388981 = A388981list(up_to);
%o A388981 A388981(n) = v388981[n];
