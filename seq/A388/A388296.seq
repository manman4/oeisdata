%o A388296 (Magma) [Min([k: k in [1..15000] | 1 + #[d: d in Divisors(k) | Modexp(d, k div d, k) mod k eq d] eq n]): n in [1..13]];
%o A388296 (Python)
%o A388296 from sympy import divisors
%o A388296 from itertools import count, islice
%o A388296 def f(n): return sum(1 for d in divisors(n, generator=True) if pow(d, n//d, n) == d%n)
%o A388296 def agen(): # generator of terms
%o A388296     adict, n = dict(), 1
%o A388296     for k in count(1):
%o A388296         v = f(k)
%o A388296         if v not in adict:
%o A388296             adict[v] = k
%o A388296             while n in adict: yield adict[n]; n += 1
%o A388296 print(list(islice(agen(), 19))) # _Michael S. Branicky_, Oct 28 2025
