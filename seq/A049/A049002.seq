%e A049002 127^2 - 2 = 16127.
%t A049002 f[n_]:=n^2-2; lst={};Do[p=Prime[n];If[PrimeQ[f[p]],AppendTo[lst,f[p]]],{n,7!}];lst (* _Vladimir Joseph Stephan Orlovsky_, Jul 16 2009 *)
