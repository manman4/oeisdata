%o A277288 def A277288_list(search_limit):
%o A277288     n, t, r = 1, Integer(3), [1]
%o A277288     while n < search_limit:
%o A277288         n += 1
%o A277288         t *= 3
%o A277288         if n.divides(t+5): r.append(n)
