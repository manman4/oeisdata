%o A277373 @cached_function
%o A277373 def L(n, x):
%o A277373     if n == 0: return 1
%o A277373     if n == 1: return 1 - x
%o A277373     return (L(n-1,x) * (2*n-1-x) - L(n-2,x)*(n-1))/n
%o A277373 A277373 = lambda n: factorial(n)*L(n, -n)
