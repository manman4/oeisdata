%o A075263 (PARI) /* Using e.g.f. A(x,y): */
%o A075263 {T(n,k)=local(X=x+x*O(x^n),Y=y+y^2*O(y^(k))); n!*polcoeff(polcoeff(-log(1-(1-exp(-X*Y))/y),n,x),k,y)}
%o A075263 for(n=0,10,for(k=0,n-1,print1(T(n,k),", "));print(""))
%o A075263 (PARI) /* Deléham's DELTA: T(n,k) = [x^(n-k)*y^k] P(n,0) */
%o A075263 {P(n,k)=if(n<0||k<0,0,if(n==0,1, P(n,k-1)+(x*(k\2+1)+y*(-(k\2+1)*((k+1)%2)))*P(n-1,k+1)))}
%o A075263 {T(n,k)=polcoeff(polcoeff(P(n,0),n-k,x),k,y)}
%o A075263 for(n=0,10,for(k=0,n,print1(T(n,k),", "));print(""))
