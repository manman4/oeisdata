%N A181669 Primes p of the form 6n-1 such that p-1 is a semiprime and p+2 is prime or prime squared.
%C A181669 Except term 7, same as A181602.
%p A181669 isA181669 := proc(n::integer)
%p A181669     if isprime(n) then
%p A181669         if modp(n,6) = 5 then
%p A181669             if numtheory[bigomega](n-1) =2 then
%p A181669                 if isprime(n+2) or issqr(n+2) and isprime(sqrt(n+2)) then
%p A181669                     true ;
%p A181669                 else
%p A181669                     false;
%p A181669                 end if;
%p A181669             else
%p A181669                 false;
%p A181669             end if;
%p A181669         else
%p A181669             false;
%p A181669         end if;
%p A181669     else
%p A181669         false;
%p A181669     end if;
%p A181669 end proc:
%p A181669 for n from 2 to 100000 do
%p A181669     if isA181669(n) then
%p A181669         printf("%d,",n) ;
%p A181669     end if;
%p A181669 end do: # _R. J. Mathar_, Oct 17 2025
