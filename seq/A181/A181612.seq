%p A181612 seq( seq(A181612(n,m),m=0..n),n=0..10) ;
%t A181612 nmax = 8; se = Series[JacobiDC[x, y], {x, 0, 2*nmax}]; t[n_, m_] := Coefficient[se, x, 2*n]*(2*n)! // Coefficient[#, y, m]& // Abs; Table[t[n, m], {n, 0, nmax}, {m, 0, n}] // Flatten (* _Jean-Fran√ßois Alcover_, Jan 09 2014 *)
