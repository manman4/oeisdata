%e A181417   1 | 1;
%e A181417   2 | 1, 2;
%e A181417   3 | 1, 12, 6;
%e A181417   4 | 1, 24, 18, 108, 24;
%e A181417   5 | 1, 40, 80, 360, 540, 960, 120;
%e A181417   6 | 1, 60, 150, 100, 900, 3600, 900, 4800, 10800, 9000, 720;
