%p A038243 PMatrix(10, n -> 5^(n-1)); # _Peter Luschny_, Oct 09 2022
%t A038243 With[{q=5}, Table[q^(n-k)*Binomial[n,k], {n,0,12}, {k,0,n}]//Flatten] (* _G. C. Greubel_, May 12 2021 *)
