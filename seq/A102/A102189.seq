%t A102189 aspartitions[n_] := Reverse /@ Sort[Sort /@ IntegerPartitions[n]]; ascycleclasses[n_Integer] := n!/(Times @@ #)& /@ ((#! Range[n]^#)& /@ Function[par, Count[par, #]& /@ Range[n]] /@ aspartitions[n]); row[n_] := ascycleclasses[n] // Reverse; Table[row[n], {n, 1, 8}] // Flatten (* _Jean-FranÃ§ois Alcover_, Feb 04 2014, after A036039 and _Wouter Meeussen_ *)
%o A102189 (PARI)
%o A102189 C(sig)={my(S=Set(sig)); vecsum(sig)!/(vecprod(sig)*prod(k=1, #S, (#select(t->t==S[k], sig))!))}
%o A102189 Row(n)={apply(C, [Vecrev(p) | p<-Vecrev(partitions(n))])}
