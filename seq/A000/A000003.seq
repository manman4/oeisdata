%t A000003 a[1] = 1; a[n_] := (k0 = k /. FindRoot[EllipticK[1-k^2]/EllipticK[k^2] == Sqrt[n], {k, 1/2, 10^-10, 1}, WorkingPrecision -> 600, MaxIterations -> 100]; Exponent[ MinimalPolynomial[RootApproximant[k0^2, 24], x], x]/2); Table[Print["a(", n, ") = ", an = a[n]]; an, {n, 1, 100}] (* _Jean-Fran√ßois Alcover_, Jul 21 2015, after _Joerg Arndt_ *)
%o A000003 (Magma) O1 := MaximalOrder(QuadraticField(D)); _,f := IsSquare(D div Discriminant(O1)); ClassNumber(sub<O1|f>);
