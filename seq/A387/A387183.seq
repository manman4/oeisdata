%N A387183 Denominators of the expected number of steps to hit the opposite corner by simple random walk on the n-cube.
%H A387183 Wikipedia, <a href="https://en.wikipedia.org/wiki/Ehrenfest_model">Ehrenfest model</a>
%H A387183 Wikipedia, <a href="https://en.wikipedia.org/wiki/Hypercube">Hypercube</a>
%F A387183 a(n) = denominator(2^(n-1) * Sum_{i=0..n-1} 1/binomial(n-1,i)).
%e A387183 1, 4, 10, 64/3, 128/3, 416/5, 2416/15, 32768/105, 21248/35, 74752/63, ...
%p A387183 t := n -> 2^(n-1)*add(1/binomial(n-1, i), i=0..n-1):
%p A387183 a := n -> denom(t(n)):
%p A387183 seq(a(n), n=1..34);
%t A387183 t[n_] := 2^(n - 1) Sum[1/Binomial[n - 1, i], {i, 0, n - 1}] // Together;
%t A387183 a[n_] := Denominator[t[n]];
%t A387183 Array[a, 34]
%o A387183 (MATLAB)
%o A387183 function q = cube_hit_den(N)
%o A387183     q = sym(zeros(1, N));
%o A387183     for n = 1:N
%o A387183         t = sym(0);
%o A387183         for i = 0:n-1
%o A387183             t = t + 1/sym(nchoosek(n-1, i));
%o A387183         end
%o A387183         t = t * sym(2)^(n-1);
%o A387183         [~, qn] = numden(t);
%o A387183         q(n) = qn;
%o A387183     end
%o A387183 end
