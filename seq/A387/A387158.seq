%t A387158 A387158Q[k_] := #[k] == #[DivisorSigma[1, k]] & [Times @@ (FactorInteger[#][[All, 1]] - 1) &];
%t A387158 Select[Range[10000], A387158Q] (* _Paolo Xausa_, Aug 20 2025 *)
