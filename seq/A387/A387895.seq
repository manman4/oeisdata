%t A387895 SemiPrimePi[ n_ ] := Sum[ PrimePi[ n/Prime @ i ] - i + 1, {i, PrimePi @ Sqrt @ n} ]; SemiPrime[ n_ ] := Block[ {e = Floor[ Log[ 2, n ] + 1 ], a, b}, a = 2^e; Do[ b = 2^p; While[ SemiPrimePi[ a ] < n, a = a + b ]; a = a - b/2, {p, e, 0, -1} ]; a + b/2 ]; a[n_]:=Permanent[ Partition[ Array[ SemiPrime, n^2 ], n ] ]; Join[{1},Array[a,13]] (* after A118770 *)
