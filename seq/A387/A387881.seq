%t A387881 prix[n_]:=If[n==1,{},Flatten[Cases[FactorInteger[n],{p_,k_}:>Table[PrimePi[p],{k}]]]];
%t A387881 suffQ[mset_]:=And@@Table[mset[[i]]<=i,{i,Length[mset]}];
%t A387881 Select[Range[100],suffQ[prix[#]]&]
