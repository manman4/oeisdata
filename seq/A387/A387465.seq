%t A387465 f[x_] := If[x == 1, {0},
%t A387465  Function[g,ReplacePart[Table[0, {PrimePi[f[[-1, 1]] ]}], #] &@
%t A387465      Map[PrimePi@ First@ # -> Last@ # &, g] ]@ FactorInteger@ x];
