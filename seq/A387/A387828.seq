%o A387828 (Python)
%o A387828 from sympy import isprime, nextprime
%o A387828 def ok(n):
%o A387828     if n < 10: return False
%o A387828     s, p, t = str(n), 2, ""
%o A387828     for i in range(len(s)-1): t, p = t + s[i] + str(p), nextprime(p)
%o A387828     return isprime(int(t+s[-1]))
%o A387828 def a(n):
%o A387828     p = nextprime(10**n)
%o A387828     while not ok(p): p = nextprime(p)
%o A387828     return p
%o A387828 print([a(n) for n in range(1, 21)]) # _Michael S. Branicky_, Sep 09 2025
