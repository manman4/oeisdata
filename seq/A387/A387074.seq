%o A387074                 if i>a:
%o A387074                     yield j
%o A387074                     a = i
%o A387074                 break
%o A387074         else:
%o A387074             for i in count(max(c,m+1)):
%o A387074                 if i not in mset and gcd(i,m)>1:
%o A387074                     if i>a:
%o A387074                         yield j
%o A387074                         a = i
%o A387074                     break
%o A387074         m = i
%o A387074         mset.add(i)
%o A387074         while c in mset:
%o A387074             mset.remove(c)
%o A387074             c += 1
