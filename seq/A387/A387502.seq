%o A387502 (Python)
%o A387502 from sympy import divisors
%o A387502 from itertools import accumulate, count, islice
%o A387502 def D(m): return set(accumulate(divisors(m)[1:]))
%o A387502 def agen(): # generator of terms
%o A387502     U = set()
%o A387502     for m in count(1):
%o A387502         debut = D(m) - U
%o A387502         yield len(debut)
%o A387502         U |= debut
%o A387502 print(list(islice(agen(), 100))) # _Michael S. Branicky_, Sep 06 2025
