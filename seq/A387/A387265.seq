%e A387265    97  149  191  251  293  347  367  397
%e A387265   127  211  223  307  331  419  431  521
%t A387265 highs := {First /@ #, Most[FoldList[Plus, 1, Length /@ #]]} &[
%t A387265 Split[Rest[FoldList[Max, -\[Infinity], #]]]] &;
%t A387265 unSortedComplement[list1_, list2_] :=
%t A387265 DeleteCases[list1, Apply[Alternatives, list2]]
%t A387265 r = 1; (* User: put your choice of r>=1 here. *)
%t A387265 rh = {};
%t A387265 rStart = Select[Map[NextPrime[r*#, -1]/# &, Prime[Range[100]]], # > 0 &];
%t A387265 AppendTo[rh, highs[rStart][[1]]];
%t A387265 While[Last[rh] =!= {}, AppendTo[rh, #[[highs[Denominator[#]][[2]]]] &[
%t A387265 highs[unSortedComplement[rStart, Flatten[rh]]][[1]]]]];
%t A387265 rh (* prime-fractions array *)
%t A387265 Denominator[rh]   (* A387265, array *)
%t A387265 Grid[Most[Denominator[rh]], Frame -> All]
