%t A387591 seq[max_] := Module[{p = Prime[Range[max]]}, p[[Position[Rest[p]/Most[p] - (1 + 1/Range[max-1]), _?Positive] // Flatten]]]; seq[115] (* _Amiram Eldar_, Sep 03 2025 *)
%o A387591 (Python)
%o A387591 from sympy import nextprime
%o A387591 def A387591_generate():
%o A387591     k, p_k, p_k_plus_1 = 1, 2, 3
%o A387591     while True:
%o A387591         if p_k * (k + 1) < k * p_k_plus_1:
%o A387591             yield p_k
%o A387591         k, p_k, p_k_plus_1 = k + 1, p_k_plus_1, nextprime(p_k_plus_1)
%o A387591 print([p_k for _, p_k in zip(range(70), A387591_generate())])
