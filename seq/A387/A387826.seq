%N A387826 a(n) = A387820(n) / A033634(A387820(n)).
%t A387826 f[p_, e_] := 1 + (p^(e + If[OddQ[e], 2, 1]) - p)/(p^2-1); s[n_] := n / Times @@ f @@@ FactorInteger[n]; s[1] = 1; Select[Array[s, 10^6], IntegerQ]
%o A387826 (PARI) s(n) = {my(f = factor(n), p = f[, 1], e = f[, 2]); prod(i = 1, #p, 1 + (p[i]^if(e[i] % 2, e[i]+2, e[i]+1) - p[i])/(p[i]^2 - 1));}
%o A387826 list(lim) = {my(sk); for(k = 1, lim, sk = s(k); if(!(k % sk), print1(k / sk, ", ")));}
