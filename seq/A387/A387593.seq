%t A387593 highs := {First /@ #, Most[FoldList[Plus, 1, Length /@ #]]} &[
%t A387593 Split[Rest[FoldList[Max, -\[Infinity], #]]]] &;
%t A387593 unSortedComplement[list1_, list2_] :=
%t A387593 DeleteCases[list1, Apply[Alternatives, list2]]
%t A387593 r = 2; (* User: put your choice of r>=1 here. *)
%t A387593 rh = {};
%t A387593 rStart = Select[Map[NextPrime[r*#, -1]/# &, Prime[Range[100]]], # > 0 &];
%t A387593 AppendTo[rh, highs[rStart][[1]]];
%t A387593 While[Last[rh] =!= {}, AppendTo[rh, #[[highs[Denominator[#]][[2]]]] &[
%t A387593 highs[unSortedComplement[rStart, Flatten[rh]]][[1]]]]];
%t A387593 rh (* prime-fractions array *)
%t A387593 Denominator[rh]   (* A387593, array *)
%t A387593 Grid[Most[Denominator[rh]], Frame -> All]
