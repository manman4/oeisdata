%o A386495 card(p)={my(c=0,u=digits(p),n=sum(i=1,#u,u[i]%2==0),even=n);while(even==n,p=nextprime(p+1);u=digits(p);even=sum(i=1,#u,u[i]%2==0);c++);c}
%o A386495 data(pp=10^9)={my(u=vector(30),r=0);forprime(p=2,pp,my(n=card(p));if(u[n]==0,u[n]=p;if(n>r,r=n)));u[1..r]}
