%o A386488 (Python)
%o A386488 from math import gcd
%o A386488 from itertools import count, islice
%o A386488 def A386488_gen(): # generator of terms
%o A386488     m, mset, c, a = 2, {2}, 3, 0
%o A386488     yield 0
%o A386488     for j in count(3):
%o A386488         for i in range(m-1,c-1,-1):
%o A386488             if i not in mset and gcd(i,m)>1:
%o A386488                 if i-j>a:
%o A386488                     yield (a:=i-j)
%o A386488                 break
%o A386488         else:
%o A386488             for i in count(max(c,m+1)):
%o A386488                 if i not in mset and gcd(i,m)>1:
%o A386488                     if i-j>a:
%o A386488                         yield (a:=i-j)
%o A386488                     break
%o A386488         m = i
%o A386488         mset.add(i)
%o A386488         while c in mset:
%o A386488             mset.remove(c)
%o A386488             c += 1
%o A386488 A386488_list = list(islice(A386488_gen(),20)) # _Chai Wah Wu_, Sep 10 2025
