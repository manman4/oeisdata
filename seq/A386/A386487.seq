%o A386487 from math import gcd
%o A386487 from itertools import count, islice
%o A386487 def A386487_gen(): # generator of terms
%o A386487     m, mset, c = 2, {2}, 3
%o A386487     yield from (0,0)
%o A386487     for j in count(3):
%o A386487         for i in range(m-1,c-1,-1):
%o A386487             if i not in mset and gcd(i,m)>1:
%o A386487                 yield i-j
%o A386487                 break
%o A386487         else:
%o A386487             for i in count(max(c,m+1)):
%o A386487                 if i not in mset and gcd(i,m)>1:
%o A386487                     yield i-j
%o A386487                     break
%o A386487         m = i
%o A386487         mset.add(i)
%o A386487         while c in mset:
%o A386487             mset.remove(c)
%o A386487             c += 1
%o A386487 A386487_list = list(islice(A386487_gen(),30)) # _Chai Wah Wu_, Sep 10 2025
