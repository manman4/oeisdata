%t A386797 f[p_, e_] := If[e == 2, 1, 0]; s[1] = 0; s[n_] := Plus @@ f @@@ FactorInteger[n]; Select[Range[2200], s[#] == 2 &]
%o A386797 (PARI) isok(k) = vecsum(apply(x -> if(x == 2, 1, 0), factor(k)[, 2])) == 2;
