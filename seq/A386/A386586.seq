%t A386586 insepQ[y_]:=Select[Permutations[y],Length[Split[#]]==Length[y]&]=={};
%t A386586 ptm[y_]:=Join@@Table[ConstantArray[k,y[[k]]],{k,Length[y]}];
%t A386586 Table[Length[Select[IntegerPartitions[n,{k}],insepQ[ptm[#]]&]],{n,0,5},{k,0,n}]
