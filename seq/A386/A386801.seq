%t A386801 f[p_, e_] := If[e == 3, 1, 0]; s[1] = 0; s[n_] := Plus @@ f @@@ FactorInteger[n]; Select[Range[17000], s[#] == 2 &]
%o A386801 (PARI) isok(k) = vecsum(apply(x -> if(x == 3, 1, 0), factor(k)[, 2])) == 2;
