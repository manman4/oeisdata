%t A386585 sepQ[y_]:=Select[Permutations[y],Length[Split[#]]==Length[y]&]!={};
%t A386585 mst[y_]:=Join@@Table[ConstantArray[k,y[[k]]],{k,Length[y]}];
%t A386585 Table[Length[Select[IntegerPartitions[n,{k}],sepQ[mst[#]]&]],{n,0,5},{k,0,n}]
