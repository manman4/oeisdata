%e A386492   0,  3/1,  4/1, 75/17, 60/13, 175/37, ...
%e A386492   ...
%e A386492 For n = 3 and m = 2: (12/5)^2 + (9/5)^2 = 3^2.
%t A386492 A[n_,m_]:=Numerator[n*(m^2-1)/(m^2+1)]; Table[A[n-m+1,m],{n,12},{m,n}]//Flatten
