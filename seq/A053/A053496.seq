%p A053496 seq(a(n), n=0..25);  # _Alois P. Heinz_, Feb 14 2013
%t A053496 a[n_] := a[n] = If[n<0, 0, If[n == 0, 1, Sum[Product[n-i, {i, 1, j-1}]*a[n-j], {j, {1, 2, 3, 6}}]]]; Table[a[n], {n, 0, 25}] (* _Jean-FranÃ§ois Alcover_, Mar 03 2014, after _Alois P. Heinz_ *)
