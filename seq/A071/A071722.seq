%p A071722 a := n -> (17*n^2 + 13*n + 6)*binomial(2*n, n)/((n + 1)*(n + 2)*(n + 3)): seq(a(n), n = 0..24);  # _Peter Luschny_, Dec 01 2024
%t A071722 a[n_] := (3*Binomial[2*n + 2, n] + 5*Binomial[2*n, n + 2])/(n + 3); Array[a, 30, 0] (* _Amiram Eldar_, Oct 04 2025 *)
