%o A353837 (SageMath) a353837 = lambda n: sum( abs(BipartiteGraph( Matrix(len(p), len(D:=list(set.union(*map(lambda t: set(divisors(t)),p)))), lambda i,j: p[i]%D[j]==0) ).matching_polynomial()[len(D)-len(p)]) for p in Partitions(n,max_slope=-1) ) # _Max Alekseyev_, Sep 11 2023
