%t A246811 a[ n_] := SeriesCoefficient[ EllipticTheta[ 3, 0, x]^2 EllipticTheta[ 2, 0, x^2] / (2 x^(1/2)), {x, 0, n}];
%t A246811 a[ n_] := SeriesCoefficient[ EllipticTheta[ 2, 0, x^(1/2)]^4 / (16 x^(1/2) EllipticTheta[ 3, 0, x^2]), {x, 0, n}];
