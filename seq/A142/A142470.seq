%t A142470 f[n_, k_]:= f[n, k]= Binomial[n, k]*Product[j!*(n+j)!/((k+j)!*(n-k+j)!), {j,1,2}];
%t A142470 T[n_, k_]:= Binomial[n, k]*f[n, k];
