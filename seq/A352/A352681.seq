%p A352681 alias(PS = ListTools:-PartialSums):
%p A352681 aList := proc(len) local a, k, P, T; a := 4; P := [1]; T := [1];
%p A352681 for k from 0 to len-1 do T := [op(T), a]; P := PS([op(P), a]); a := P[-1] od;
%p A352681 T end: aList(27);
