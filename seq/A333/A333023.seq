%o A333023 (PARI) m = 6; v = vector(m); for(k=1, m, v[k] = istotient(2*k)); for(k = m+1, 1.5e5, if(Set(v) == [1], print1(2*(k-m),", ")); v = concat(v[2..m], istotient(2*k)))
