%N A290094 Lexicographically earliest infinite sequence such that a(i) = a(j) => A278222(A289813(i)) = A278222(A289813(j)) and A278222(A289814(i)) = A278222(A289814(j)) for all i, j >= 0.
%C A290094 Restricted growth sequence transform of A290093, i.e., of the ordered pair [A290091(n), A290092(n)].
%C A290094 From _Antti Karttunen_, Oct 21 2025: (Start)
%C A290094 For all i, j >= 0:
%C A290094   A331303(i) = A331303(j) => a(i) = a(j),
%C A290094   a(i) = a(j) => A387188(i) = A387188(j) => A006047(i) = A006047(j) => A053735(i) = A053735(j).
%C A290094 (End)
%H A290094 Antti Karttunen, <a href="/A290094/b290094.txt">Table of n, a(n) for n = 0..59049</a>
%o A290094 (PARI)
%o A290094 up_to = 59049;
%o A290094 rgs_transform(invec) = { my(om = Map(), outvec = vector(length(invec)), u=1); for(i=1, length(invec), if(mapisdefined(om,invec[i]), my(pp = mapget(om, invec[i])); outvec[i] = outvec[pp] , mapput(om,invec[i],i); outvec[i] = u; u++ )); outvec; };
%o A290094 A005940(n) = { my(p=2, t=1); n--; until(!n\=2, if((n%2), (t*=p), p=nextprime(p+1))); t };
%o A290094 A046523(n) = { my(f=vecsort(factor(n)[, 2], , 4), p); prod(i=1, #f, (p=nextprime(p+1))^f[i]); };
%o A290094 A278222(n) = A046523(A005940(1+n));
%o A290094 A289813(n) = { my(d=digits(n, 3)); fromdigits(vector(#d, i, if (d[i]==1, 1, 0)), 2); };
%o A290094 A289814(n) = { my(d=digits(n, 3)); fromdigits(vector(#d, i, if (d[i]==2, 1, 0)), 2); };
%o A290094 Aux290094(n) = [A278222(A289813(n)), A278222(A289814(n))];
%o A290094 v290094 = rgs_transform(vector(1+up_to,n,Aux290094(n-1)));
%o A290094 A290094(n) = v290094[1+n]; \\ _Antti Karttunen_, Oct 21 2025
