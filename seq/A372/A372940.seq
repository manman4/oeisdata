%t A372940 seq[kmax_] := Module[{f0 = 1, f1 = 2, f2, s = {1}}, Do[f2 = ((7*k^2 - 7*k + 2)*f1 + 8*(k-1)^2*f0)/k^2; If[Divisible[f2, k], AppendTo[s, k]]; f0 = f1; f1 = f2, {k, 2, kmax}]; s]; seq[5000]
