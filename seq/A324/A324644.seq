%F A324644 a(n) = gcd(A000203(n), A276086(n)).
%o A324644 (PARI)
%o A324644 A276086(n) = { my(m=1, p=2); while(n, m *= (p^(n%p)); n = n\p; p = nextprime(1+p)); (m); };
%o A324644 A324644(n) = gcd(sigma(n),A276086(n));
%o A324644 (PARI) A324644(n) = { my(m=1, p=2, s=sigma(n)); while(n, m *= (p^min(n%p, valuation(s, p))); n = n\p; p = nextprime(1+p)); (m); }; \\ _Antti Karttunen_, Sep 17 2025
