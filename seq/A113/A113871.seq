%o A113871 (SageMath)
%o A113871 def A113871_list(len):
%o A113871     R, C = [1], [1]+[0]*(len-1)
%o A113871     for n in (1..len-1):
%o A113871         for k in range(n,-1,-1):
%o A113871             C[k] = C[k-1] * k^2
%o A113871         C[0] = -sum(C[k] for k in (1..n))
%o A113871         R.append(C[0])
%o A113871     return R
