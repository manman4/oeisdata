%o A113300 (SageMath)
%o A113300 @CachedFunction
%o A113300 def T(n): # T(n) = A000073(n)
%o A113300     if (n<2): return 0
%o A113300     elif (n==2): return 1
%o A113300     else: return T(n-1) +T(n-2) +T(n-3)
%o A113300 def a(n): return sum( T(2*j) for j in (0..n) )
