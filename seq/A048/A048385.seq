%o A048385 (Python)
%o A048385 def digits(n):
%o A048385     d=[]
%o A048385     while n>0:
%o A048385         d.append(n%10)
%o A048385         n=n//10
%o A048385     return d
%o A048385 def sqdig(n):
%o A048385     new=0
%o A048385     num=digits(n)
%o A048385     spacing=0
%o A048385     while num:
%o A048385         k=num.pop(0)
%o A048385         new+=(10**(spacing))*(k**2)
%o A048385         if k>3:
%o A048385             spacing+=1
%o A048385         spacing+=1
%o A048385     return new
%o A048385 # _David Nacin_, Aug 19 2012
