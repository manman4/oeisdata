%e A390021 3*(3-7) = 6*(6-8), so (3,7,6,8) is in the list.
%t A390021 solnsM[m_, max_] := Module[{ans = {}, rhs = {}, u, v, w, lhs, matching},
%t A390021    Do[Do[AppendTo[rhs, {v*(v - w), v, w}], {w, max}], {v, m*(m + max)}];
%t A390021    rhs = GatherBy[rhs, First];
%t A390021    Do[lhs = m*(m - u);
%t A390021     matching = Select[rhs, #[[1, 1]] == lhs &];
%t A390021     If[Length[matching] > 0,
%t A390021      Do[AppendTo[ans, Map[{m, u, #[[2]], #[[3]]} &, matching[[1]]]], {i,
%t A390021        Length[matching]}]], {u, max}];
%t A390021    ans = Flatten[ans, 1];
%t A390021    Select[Union[Map[Sort[{#, RotateLeft[#, 2]}][[1]] &,
%t A390021       Sort[Select[DeleteDuplicates[ans], Length[Union[#]] == 4 &]]]], #[[1]] == m &]];
%t A390021 TableForm[solns = solnsM[3, 100],
%t A390021 TableHeadings -> {None, {"m", "u", "v", "w"}}]
%t A390021 Map[#[[2]] &, solns] (* u, A390021 *)
%t A390021 Map[#[[3]] &, solns] (* v, A390022 *)
%t A390021 Map[#[[4]] &, solns] (* w, A390023 *)
