%e A080054 The sequence of the 10th partial denominators [33,110,259,504,...], starting at m = 2, appears to be given by the polynomial 4*m^3 + m - 1.
%e A080054 The sequence of the 15th partial denominators [15,78,23,110,31,142,...], starting at m = 4, appears to be quasi-polynomial in m, with constituent polynomials 4*m - 1 and 16*m - 2. (End)
%p A080054 b:= proc(n, i) option remember; `if`(n=0, 1, `if`(i<1, 0,
%p A080054       b(n, i-2) +add(2*b(n-i*j, i-2), j=1..n/i)))
%p A080054     end:
%p A080054 a:= n-> b(n, n-1+irem(n, 2)):
%p A080054 seq(a(n), n=0..50);  # _Alois P. Heinz_, Feb 10 2014
%p A080054 # alternative program using expansion of f(x, x^3) / f(-x, -x^3):
%p A080054 with(gfun): series( add(x^(n*(2*n-1)), n = -8..8)/add((-1)^n*x^(n*(2*n-1)), n = -8..8), x, 100): seriestolist(%); # _Peter Bala_, Feb 05 2021
%t A080054 a[ n_] := With[ {m = InverseEllipticNomeQ @ q}, SeriesCoefficient[ (1 - m )^(-1/8), {q, 0, n}]]; (* _Michael Somos_, Aug 03 2011 *)
%t A080054 a[ n_] := SeriesCoefficient[ (EllipticTheta[ 3, 0, q] / EllipticTheta[ 4, 0, q])^(1/2), {q, 0, n}]; (* _Michael Somos_, Aug 03 2011 *)
%t A080054 a[ n_] := SeriesCoefficient[ QPochhammer[ -q] / QPochhammer[ q], {q, 0, n}]; (* _Michael Somos_, May 10 2014 *)
%t A080054 a[ n_] := SeriesCoefficient[ QHypergeometricPFQ[ {-1}, {}, q^2, q], {q, 0, n}]; (* _Michael Somos_, May 10 2014 *)
%t A080054 b[n_, i_] := b[n, i] = If[n == 0, 1, If[i < 1, 0, b[n, i - 2] + Sum[2*b[n - i*j, i - 2], {j, 1, n/i}]]];
%t A080054 a[n_] := b[n, n - 1 + Mod[n, 2]];
%t A080054 Table[a[n], {n, 0, 50}] (* _Jean-Fran√ßois Alcover_, Nov 05 2017, after _Alois P. Heinz_ *)
%o A080054 (PARI) {a(n) = my(A, m); if( n<0, 0, m=1; A = 1 + 2*x + O(x^2); while( m<n, m*=2; A = subst(A, x, x^2); A = sqrt((A^4 + 1) / 2) + 2 * sqrt((A^4 - 1) / 8)); polcoeff(A, n))};
%o A080054 (PARI) a(n)=polcoeff(exp(2*sum(k=0,n\2,sigma(2*k+1)/(2*k+1)*x^(2*k+1))),n) /* _Paul D. Hanna_ */
%o A080054 (PARI) {a(n) = my(A); if( n<0, 0, A = x * O(x^n); polcoeff( eta(x^2 + A)^3 / (eta(x + A)^2 * eta(x^4 + A)), n))}; /* _Michael Somos_, Jul 07 2005 */
