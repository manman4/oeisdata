%C A254407 s=3: A027480(n) = (n+1)*A000217(n+1) - Sum_{i=1..n+1} i;
%C A254407 s=4: A162148(n) = (n+1)*A000290(n+1) - Sum_{i=1..n+1} A000217(i);
