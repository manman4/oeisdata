%F A249747 a(n) = floor((A002110(n) * A054272(n)) / A001248(n)).
%F A249747 The ratio a(n) / A005867(n) seems to stay near 1. Note that A005867(n) = A000010(A002110(n)). See also formulas in A054272.
%o A249747 (Scheme) (define (A249747 n)  (floor->exact (/ (* (A002110 n) (A054272 n)) (A001248 n))))
%o A249747 (PARI)
%o A249747 default(primelimit, 2^31 + 2^30);
%o A249747 A002110(n) = prod(i=1, n, prime(i));
%o A249747 A054272(n) = 1 + primepi(prime(n)^2) - n;
%o A249747 A249747(n) = (A054272(n)*A002110(n))\(prime(n)^2);
%o A249747 for(n=1, 100, write("b249747.txt", n, " ", A249747(n)));
